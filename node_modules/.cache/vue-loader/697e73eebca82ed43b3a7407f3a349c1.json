rorDescription; \r\n                this.dialog_new=false; \r\n         })\r\n      },\r\n      onUpload(){\r\n           \r\n           const uploadedFileDatas = new FormData();\r\n              console.log(this.uploadedFileDatas);\r\n              uploadedFileDatas.append('uploadedDocumentData', new Blob([JSON.stringify( {description: this.description})], {type : 'application/json'}));  \r\n              uploadedFileDatas.append('uploadedDocumentFile', this.selectedFile, this.selectedFile.fname);  \r\n\r\n               uploadedFileDatas.contractId=this.contractId;\r\n              this.addContractDocuments(uploadedFileDatas)\r\n               .then(()=>{\r\n               this.dialog_new=false;\r\n               console.log(uploadedFileDatas);\r\n                this.fetchContractDocuments(this.contractId);\r\n               })  \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data.errorDescription; \r\n                this.dialog_new=false; \r\n         })\r\n             \r\n            }\r\n\r\n       \r\n   \r\n   },\r\n    created(){\r\n      this.fetchContractDocuments(this.contractId);\r\n    }\r\n   \r\n  \r\n}\r\n</script>\r\n\r\n <style scoped>\r\n.iframe_dialog img{\r\n  width:100%!important;\r\n   height:100%!important;\r\n}\r\n #list .row{\r\n   align-items: center;\r\n }\r\n\r\n  .txtarea label{\r\n    font-size: 12px!important;\r\n }\r\n .file_button{\r\n       width: 100%!important;\r\n    border: 1px solid #e4e4e3;\r\n        font-size: 16px!important;\r\n    letter-spacing: unset!important;\r\n \r\n    color: rgba(0, 0, 0, 0.6);\r\n    overflow: hidden;\r\n }\r\n.file_add{ \r\n    margin-top: 10px;\r\n    text-align: right;\r\n}\r\n.file_add button{\r\n    background: #14b6db!important;\r\n    color: white;}\r\n \r\n#itmList{\r\n   border: 1px solid #ecebeb;\r\n   margin-bottom: 8px;\r\n}\r\n\r\n.toAdd{\r\n  display: none;\r\n}\r\n\r\n.downloadFile{\r\n      min-widt