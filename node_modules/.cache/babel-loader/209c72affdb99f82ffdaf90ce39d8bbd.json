item> \r\n           </v-list>\r\n    </v-menu>\r\n    </td>\r\n        </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n  </div>\r\n\r\n</template>\r\n \r\n<script>\r\nimport CancelPersonnelPos from '../popups/CancelPersonnelPos';\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nexport default {\r\n  components:{\r\n    CancelPersonnelPos\r\n  },\r\n    data () {\r\n      return {\r\n          \r\n      }\r\n    },\r\n      methods: {\r\n    ...mapActions([\"fetchPosition\"]),\r\n    clickFunction() {\r\n       var tr = document.querySelectorAll('.table-contract tr'); \r\n      \r\n       for (var i = 0; i < tr.length; i++) {\r\n     \r\n       \r\n         tr[i].addEventListener(\"click\", function(){   \r\n          if(document.querySelector('.table-contract tr.active_tr')!=null){\r\n           document.querySelector('.table-contract tr.active_tr').classList.remove('active_tr');}\r\n            this.classList.add('active_tr')\r\n       }, false\r\n         )}\r\n       }\r\n        },\r\n  computed: mapGetters([\"allPosition\"]),\r\n  created() {\r\n    this.fetchPosition()\r\n    .then(()=>{\r\n      this.clickFunction();\r\n    })\r\n     .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'){ this.$router.push({ path: '/' });   }  \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"],"sourceRoot":"src/components"}]}