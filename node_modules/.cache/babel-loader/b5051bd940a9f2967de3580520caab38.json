ader\"\r\n           \r\n        >\r\n          <span>Ödənişi redaktə et</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n          <v-form  @submit=\"updatePaymentSubmit\"  v-model=\"isFormValid\" ref=\"form\" >\r\n        <v-card-text>\r\n               <v-row>\r\n                 \r\n \r\n\r\n                                 <v-col cols=\"12\"  md=\"12\">\r\n                          <v-menu\r\n                              v-model=\"menu\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.paymentDate\"\r\n                              label=\"Ödəniş tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker v-model=\"formatpaymentDate\" @input=\"menu = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col>\r\n                <v-col cols=\"12\"  md=\"12\"   >\r\n                         <v-text-field label=\"Ödənişin miqdarı\"  v-model=\"formData.paymentAmount\" required ></v-text-field>\r\n                         \r\n                         </v-col>\r\n               \r\n             </v-row>\r\n        </v-card-text>\r\n    <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n        <v-divider></v-divider>\r\n \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; fetchPayment(contractId); \"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\"; \r\n import ErrorDialog from '../popups/ErrorDialog';\r\n  export default {\r\n    components:{ErrorDialog},\r\n  props:[\"contractId\", \"paymentId\"],\r\n    data () {\r\n      return {\r\n         formData:{ paymentDate:null },\r\n         isFormValid:false,\r\n         dialog: false,\r\n         menu:false,\r\n         validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n           errorMsg:'',\r\n        dialog_err:false,\r\n           router: this.$router.currentRoute.path,\r\n      }\r\n    },\r\n    methods:{\r\n...mapActions([\"fetchPayment\", \"updatePayment\", \"getContract\",\"fetchContracts\"]),\r\n  getP(){ \r\n    this.fetchPayment(this.contractId)\r\n     .then(()=>{\r\n            this.formData=this.allpayments.find(item => item.id === this.paymentId); \r\n  \r\n            console.log(typeof this.formData.paymentDate);\r\n                 console.log(this.formData);\r\n          })\r\n  \r\n  },\r\n\r\n         updatePaymentSubmit(e) {   \r\n          e.preventDefault();\r\n            this.formData.contractId=this.contractId;\r\n            this.formData.paymentId=this.paymentId;\r\n            this.updatePayment(this.formData)\r\n              .then(()=>{\r\n                    this.fetchContracts(this.actionQueryString);\r\n                \r\n                this.fetchPayment(this.contractId);\r\n                this.getContract(this.contractId);\r\n                this.dialog=false; \r\n              })  \r\n             .catch(error => {  \r\n                      \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        },\r\n  },\r\n     computed: {\r\n       ...mapGetters([\"allpayments\",\"loginuser\"]), \r\n         actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n       formatpaymentDate:\r\n        {   \r\n              get() { \r\n               if (this.formData.contractDate === undefined) { return null; }\r\n                return  this.formData.paymentDate.toString()}  ,\r\n              set(value) { this.formData.paymentDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n    \r\n     },\r\n   \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n.new-payment{\r\n  text-align: center!important;\r\n}\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.edit-but{\r\n     color: #2294f2!important;\r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n     text-transform: uppercase!important;\r\n}\r\n\r\n.btn-new-p{\r\n      padding: 0px 15px!important;\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"],"sourceRoot":"src/popups"}]}