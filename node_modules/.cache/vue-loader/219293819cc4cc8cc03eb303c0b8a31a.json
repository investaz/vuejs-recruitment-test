\n                             <v-text-field\r\n                              :value=\"formData.entranceDate\" \r\n                              label=\"İşə qəbul tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"  \r\n                               class=\"required\"\r\n                              ></v-text-field>\r\n                          </template>\r\n                         \r\n                          <v-date-picker  :max=\"nowDate\" :first-day-of-week=\"1\" v-model=\"formatenterancedate\" @input=\"menu2 = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                         </v-col>\r\n                         <v-col   cols=\"12\"   md=\"4\" > \r\n                           <v-select :rules=\"validationRules\" class=\"required\"   v-model=\"formData.personnelPositionId\"    label=\"Vəzifəsi\" :items=\"emptyP\" item-value=\"id\" item-text=\"description\">\r\n                            \r\n                             </v-select>   \r\n                        </v-col>\r\n                         \r\n                           <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field   v-model=\"formData.faxNumber\" label=\"Faks nömrəsi\"  ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                             <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field class=\"required\"    :rules=\"validationRules\" v-model=\"formData.phoneNumber\" label=\"Telefon nömrəsi\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                           <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field class=\"required\" :rules=\"validationRules\" v-model=\"formData.pinCode\" maxLength=\"7\" label=\"FİN kodu\"   required >\r\n                                     <v-tooltip bottom  slot=\"append\">\r\n      <template v-slot:activator=\"{ on, attrs }\">\r\n        \r\n         <v-icon v-on=\"on\" color=\"primary\" dark  v-bind=\"attrs\">\r\n                  forum\r\n                </v-icon>\r\n      </template>\r\n     <div class=\"align-center\">\r\n        <v-img \r\n          class=\"shrink mr-2\"\r\n          contain\r\n          src=\"../assets/tooltip.png\"\r\n          transition=\"scale-transition\"\r\n          width=\"500\"\r\n        />\r\n     </div>\r\n    </v-tooltip>\r\n                         </v-text-field>\r\n                        \r\n                         </v-col>\r\n                      </v-row>\r\n                 \r\n          <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; reset()\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn   :disabled=\"!isFormValid\"   type=\"submit\" class=\"btn-add\" >ƏLAVƏ ET</v-btn> \r\n             \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\n// import jQuery from 'jquery';\r\n//     let $ = jQuery;\r\n// import DatePicker from 'vue2-datepicker'\r\nimport 'vue2-datepicker/index.css';\r\n//  import 'vue2-datepicker/locale/aze';\r\nimport { mapGetters, mapActions } from \"vuex\";  \r\n  export default {\r\n    components:{ErrorDialog },\r\n    data () {\r\n      return {\r\n        nowDate: new Date().toISOString().slice(0,10),\r\n        yesterday:new Date(new Date().setDate(new Date().getDate()-1)).toISOString().slice(0,10),\r\n        date: new Date(),  \r\n        formData:{phoneNumber:'+994'},\r\n        errorMsg:'',\r\n        dialog_err:false,\r\n        dialog: false,\r\n        menu1:false, \r\n        menu2:false,  \r\n        isFormValid:false, \r\n        validationRules: [\r\n        v => !!v || 'Sahəni boş buraxmaq olmaz!', \r\n         ],\r\n           validationEmailRules: [\r\n        v => /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(v) || 'Email düzgün qeyd olunmayıb!', \r\n         ],\r\n         emptyP:[]\r\n      }\r\n    },\r\n\r\n     computed:{\r\n      \r\n        formatbirthdate:\r\n        {\r\n            get() {\r\n              if (this.formData.birthDate === undefined) { return null; }\r\n              return  this.formData.birthDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.birthDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n         formatenterancedate:\r\n        {\r\n            get() {\r\n              if (this.formData.entranceDate === undefined) { return null; }\r\n              return  this.formData.entranceDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.entranceDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n     \r\n    ...mapGetters([\"allPosition\" ]),\r\n\r\n        \r\n      },\r\n \r\n  methods: {\r\n    \r\n  \r\n     ...mapActions([\"fetchPosition\", \"addWorker\",\"fetchWorkers\", \"fetchPositionCombo\" ]),\r\n       reset() { this.$refs.form.reset();  }, \r\n\r\n        addWorkerSubmit(e) {    \r\n        e.preventDefault(); \r\n        this.addWorker(this.formData)\r\n        .then(()=>{ \r\n         \r\n         this.reset();\r\n         this.fetchWorkers()\r\n         .then(()=>{\r\n            \r\n         this.dialog=false; \r\n         })\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        \r\n       },\r\n\r\nfetchEmptyPos(){\r\n  this.fetchPositionCombo()\r\n  .then((res)=>{\r\n     this.emptyP=res.data.filter(pos=>{return pos.personnelId === undefined});\r\n    \r\n  })\r\n}\r\n\r\n        },\r\n \r\n  }  \r\n  \r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>\r\n\r\n"]}]}