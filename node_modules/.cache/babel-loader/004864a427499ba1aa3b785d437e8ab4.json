t:activator=\"{ on }\">\r\n                             <v-text-field\r\n                               :value=\"formData.contractDateBegin \"\r\n                                label=\"Başlanğıc tarix\"\r\n                                append-icon=\"event\"\r\n                                readonly\r\n                               v-on=\"on\" \r\n                               outlined\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker   v-model=\"formatcontractDateBegin\" @input=\"menu1 = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col>\r\n                         <v-col  cols=\"12\" sm=\"6\" md=\"3\"  >\r\n                          <v-menu\r\n                              v-model=\"menu2\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                               :value=\"formData.contractDateEnd\"\r\n                                label=\"Son tarix\"\r\n                                append-icon=\"event\"\r\n                                readonly\r\n                               v-on=\"on\" \r\n                               outlined\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker  v-model=\"formatcontractDateEnd\" @input=\"menu2 = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col>\r\n\r\n                      <v-col cols=\"12\"  md=\"3\"   >\r\n                         <v-text-field outlined label=\"Müqavilənin nömrəsi\"  v-model=\"formData.contractNumberSearchText\"   ></v-text-field>\r\n                      \r\n                         </v-col>\r\n                     <v-col cols=\"12\"  md=\"3\"   >\r\n                         <v-text-field outlined label=\"Qeydiyyat №\"  v-model=\"formData.registrationNumberSearchText\" required ></v-text-field>\r\n                         \r\n                         </v-col>\r\n\r\n        <v-col  cols=\"12\"   md='4'>\r\n        \r\n           <v-select  outlined label=\"Müqavilənin mərhələsi\" :items=\"allcontractProcessState\" item-value=\"code\" item-text=\"description\" v-model=\"formData.processStatus\"> </v-select> \r\n      </v-col>\r\n                          <v-col   cols=\"12\"   md=\"4\" > \r\n                          <v-text-field outlined label=\"Obyektin ünvanı\"   required v-model=\"formData.soldAssetAddressAddressSearchText\"></v-text-field>\r\n                     </v-col>\r\n\r\n     <v-col   cols=\"12\"   md=\"4\" > \r\n                          <v-text-field outlined label=\"Obyektin adı\"   required v-model=\"formData.soldAssetNameSearchText\"></v-text-field>\r\n                     </v-col>\r\n                        <v-col cols=\"12\"  md=\"4\"  >\r\n                            <div class=\"buyerinfo\">\r\n                             <input class=\"left-input\" placeholder=\"Alıcının adı\"   v-model=\"formData.buyerNameSearchText\"> \r\n                             <input class=\"left-input\" placeholder=\"Soyadı\"    v-model=\"formData.buyerSurnameSearchText\"> \r\n                             <input class=\"left-input\" placeholder=\"Ata adı\"    v-model=\"formData.buyerPatronymicSearchText\"> \r\n                             </div>\r\n                         </v-col>\r\n\r\n                            <v-col cols=\"12\"  md=\"4\" class=\"debitorbox\" >\r\n                        <v-checkbox\r\n                              label=\"Yalnız borcu