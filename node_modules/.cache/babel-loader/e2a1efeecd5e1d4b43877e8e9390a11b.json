\n          <v-icon>create_new_folder</v-icon>\r\n          <span>Yeni</span>\r\n         </v-btn>\r\n         \r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span> İnsan resursları > Əməkdaşlar > Yeni əməkdaş yarat</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n          <v-form  @submit=\"addWorkerSubmit\"  v-model=\"isFormValid\" ref=\"form\" >\r\n        <v-card-text>\r\n                   <v-row>\r\n                  \r\n                         <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.name\" label=\"Adı\"   required ></v-text-field>\r\n                   \r\n                         </v-col>\r\n                      \r\n                         <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field :rules=\"validationRules\"  v-model=\"formData.surname\" label=\"Soyadı\"   required ></v-text-field>\r\n                     \r\n                         </v-col>\r\n                           \r\n                            <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.patronymic\" label=\"Ata adı\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                      \r\n                          <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.emailAddress\" label=\"Email\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                        <v-col cols=\"12\"  md=\"4\"> \r\n                         <v-menu\r\n                              v-model=\"menu1\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.birthDate\" \r\n                              label=\"Doğum tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"  \r\n                              ></v-text-field>\r\n                          </template>\r\n                         \r\n                          <v-date-picker  v-model=\"formatbirthdate\" @input=\"menu1 = false\"></v-date-picker>\r\n                          </v-menu> \r\n                         </v-col>\r\n                        <v-col cols=\"12\"  md=\"4\"> \r\n                         <v-menu\r\n                              v-model=\"menu2\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.entranceDate\" \r\n                              label=\"İşə qəbul tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"  \r\n                              ></v-text-field>\r\n                          </template>\r\n                         \r\n                          <v-date-picker  v-model=\"formatenterancedate\" @input=\"menu2 = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                         </v-col>\r\n                         <v-col   cols=\"12\"   md=\"4\" > \r\n                           <v-select :rules=\"validationRules\"  v-model=\"formData.personnelPositionId\"    label=\"Vəzifəsi\" :items=\"emptyP\" item-value=\"id\" item-text=\"description\">\r\n                            \r\n                             </v-select>   \r\n                        </v-col>\r\n                         \r\n                           <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.faxNumber\" label=\"Faks nömrəsi\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                             <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.phoneNumber\" label=\"Telefon nömrəsi\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                           <v-col cols=\"12\"  md=\"4\"   >\r\n                         <v-text-field  :rules=\"validationRules\" v-model=\"formData.pinCode\" label=\"FİN kodu\"   required ></v-text-field>\r\n                        \r\n                         </v-col>\r\n                      </v-row>\r\n                 \r\n          <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; reset()\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn   :disabled=\"!isFormValid\"   type=\"submit\" class=\"btn-add\" >ƏLAVƏ ET</v-btn> \r\n             \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\nimport { mapGetters, mapActions } from \"vuex\";  \r\n  export default {\r\n    components:{ErrorDialog},\r\n    data () {\r\n      return {\r\n        formData:{},\r\n        errorMsg:'',\r\n        dialog_err:false,\r\n        dialog: false,\r\n        menu1:false, \r\n        menu2:false,  \r\n        isFormValid:false, \r\n        validationRules: [\r\n        v => !!v || 'Sahəni boş buraxmaq olmaz!', \r\n         ],\r\n         emptyP:[]\r\n      }\r\n    },\r\n\r\n     computed:{\r\n        formatbirthdate:\r\n        {\r\n            get() {\r\n              if (this.formData.birthDate === undefined) { return null; }\r\n              return  this.formData.birthDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.birthDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n         formatenterancedate:\r\n        {\r\n            get() {\r\n              if (this.formData.entranceDate === undefined) { return null; }\r\n              return  this.formData.entranceDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.entranceDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n    ...mapGetters([\"allPosition\" ]),\r\n\r\n        emptyPos:function () {\r\n           return this.allPosition.filter(pos=>{return pos.personnelId === null});\r\n        }\r\n      },\r\n \r\n  methods: {\r\n     ...mapActions([\"fetchPosition\", \"addWorker\",\"fetchWorkers\" ]),\r\n       reset() { this.$refs.form.reset();  }, \r\n\r\n        addWorkerSubmit(e) {    \r\n        e.preventDefault(); \r\n        this.addWorker(this.formData)\r\n        .then(()=>{ \r\n         \r\n         this.reset();\r\n         this.fetchWorkers()\r\n         .then(()=>{\r\n            \r\n         this.dialog=false; \r\n         })\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        \r\n       },\r\n\r\nfetchEmptyPos(){\r\n  this.fetchPosition()\r\n  .then(()=>{\r\n     this.emptyP=this.allPosition.filter(pos=>{return pos.personnelId === undefined});\r\n     console.log(this.allPosition.filter(pos=>{return pos.personnelId === undefined}));\r\n  })\r\n}\r\n\r\n        },\r\n     \r\n \r\n  }  \r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/popups"}]}