='filterdiv' align=\"center\">\r\n      <v-col v-if=\"departmentFilter\" class=\"d-flex form-item\" cols=\"12\" sm=\"5\" md='3'>\r\n        \r\n    <v-select  outlined label=\"Şöbələr\" :items=\"allOrgs\" item-value=\"id\" item-text=\"description\"> </v-select> \r\n      </v-col>\r\n       <v-col :title=\"getContTypeTitle(conType)\" v-if=\"contractTypeFilter\" class=\"d-flex form-item\" cols=\"12\" sm=\"5\" md='3'>\r\n       \r\n        <v-select v-model=\"conType\" outlined label=\"Müqavilənin tipi\" :items=\"filterArray\" item-value=\"id\" item-text=\"description\" @change=\"filterArrayFun(conType)\">\r\n    \r\n           </v-select>\r\n      \r\n\r\n      </v-col>\r\n      <v-spacer></v-spacer>\r\n     \r\n     \r\n       <div v-if=\"newbuttonWorkers\" class=\"buttons-bar\" cols=\"12\" sm=\"2\" md='1'>\r\n           <NewWorker/>\r\n      </div>\r\n        <div v-if=\"newbuttonSellers\" class=\"buttons-bar\" cols=\"12\" sm=\"2\" md='1'>\r\n           <NewSeller/>\r\n      </div>\r\n        <div v-if=\"newbuttonpersonnelPos\" class=\"buttons-bar\" cols=\"12\" sm=\"2\" md='1'>\r\n           <NewPersonnelPos/>\r\n      </div>\r\n          <div v-if=\"newbuttonrole\" class=\"buttons-bar\" cols=\"12\" sm=\"2\" md='1'>\r\n           <NewRole/>\r\n        </div>\r\n\r\n           <div v-if=\"newbuttonFeeTariffs\" class=\"buttons-bar\" cols=\"12\" sm=\"2\" md='1'>\r\n           <NewFeeTariff/>\r\n      </div>\r\n    </v-row>\r\n</template>\r\n\r\n<script>\r\nimport NewWorker from '../popups/NewWorker';\r\nimport NewSeller from '../popups/NewSeller';\r\nimport NewPersonnelPos from '../popups/NewPersonnelPos';\r\nimport NewRole from '../popups/NewRole';\r\nimport NewFeeTariff from '../popups/NewFeeTariff';\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nexport default {\r\n     components:{NewWorker, NewSeller,NewPersonnelPos, NewRole, NewFeeTariff},\r\n    name:\"Filterbar\",\r\n       data () {\r\n      return {\r\n         router: this.$router.currentRoute.path,\r\n        // type:null,\r\n        contt:[{\r\n            code: \"Hamısı\",\r\n            description: \"Hamısı\",\r\n            id: 100}\r\n        ],\r\n        \r\n         filterArray:[]\r\n      }\r\n    },\r\n    computed:{\r\n    ...mapGetters([\"allOrgs\", \"allcontracttype\",\"loginuser\"]),\r\n\r\n    conType:\r\n      {\r\n        get () { return this.$store.getters.conType },\r\n        set (value) { this.$store.commit('updateType', value) }\r\n      },\r\n    departmentFilter() {\r\n        return this.$route.meta.departmentFilter;\r\n    },\r\n    contractTypeFilter() {\r\n        return this.$route.meta.contractTypeFilter;\r\n    },\r\n  \r\n    newbuttonWorkers() {\r\n        return this.$route.meta.newbuttonWorkers;\r\n    },\r\n      newbuttonSellers() {\r\n        return this.$route.meta.newbuttonSellers;\r\n    } ,\r\n      newbuttonpersonnelPos() {\r\n        return this.$route.meta.newbuttonpersonnelPos;\r\n    },\r\n       newbuttonrole() {\r\n        return this.$route.meta.newRole;\r\n    },\r\n       newbuttonFeeTariffs() {\r\n        return this.$route.meta.newFeeTariffs;\r\n    }, \r\n\r\n     actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n    },\r\n    methods: {\r\n     ...mapActions([\"fetchUnitOrgs\",  \"fetchContType\", \"fetchContracts\"]),\r\n\r\n      getContTypeTitle(id){   \r\n                    if(this.conType && this.conType!=100){\r\n                  return  this.allcontracttype.find(x => x.id == id).description;\r\n                      }\r\n                      else{\r\n                        return \"\"\r\n                      }\r\n                  },\r\n\r\n     filterArrayFun(t){\r\n          if(t!=100){\r\n           return this.fetchContracts(this.actionQueryString+'&&contractTypeId='+ t)\r\n          }\r\n          else{\r\n            return this.fetchContracts(this.actionQueryString)\r\n          }\r\n     }\r\n       \r\n     },\r\n \r\n      created() {\r\n        //  this.fetchUnitOrgs(); \r\n       this.fetchContType()\r\n       .then(()=>{\r\n         this.filterArray=this.contt.concat(this.allcontracttype);\r\n        \r\n       }) \r\n\r\n       \r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n     .filterdiv{\r\n          display: flex; \r\n           margin: 0px!important;\r\n          box-shadow: 0 0 13px 0 rgba(0, 0, 0, 0.15);\r\n           background-color: #ffffff;\r\n           margin: 10px 0px!important;\r\n     }\r\n     .v-input{\r\n         font-size: 14px!important;\r\n     }\r\n      .v-input__append-inner{\r\n              margin-top: 4px!important;\r\n      }\r\n      .v-select__selections{\r\n         padding: 0px!important;\r\n\r\n      }\r\n      .v-input__slot{\r\n          min-width: unset!important;\r\n             margin-bottom: 0px!important;\r\n              min-height: unset!important;\r\n      }\r\n      .v-text-field.v-text-field--enclosed .v-text-field__details\r\n       {\r\n      margin-bottom: 0px!important;  \r\n}\r\n\r\n .v-text-field--outlined .v-label {\r\n    top: 6px!important;\r\n}\r\n .filterdiv .v-label.v-label--active.theme--light.primary--text {\r\n    top:18px!important\r\n}  \r\n\r\n .filterdiv .v-label.v-label--active.theme--light {\r\n    top:18px!important\r\n} \r\n\r\n.form-item{\r\n    padding-bottom: 0px!important;\r\n}\r\n\r\n.buttons-bar   { \r\n  text-align: center!important;\r\n  padding: 6px 5px;\r\n}\r\n\r\n.buttons-bar a,.buttons-bar button{ \r\n  color: #2294F2!important;\r\n}\r\n\r\n.new_button span{\r\n  display: block;\r\n  text-align: center!important;\r\n  color: #2294F2!important;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}