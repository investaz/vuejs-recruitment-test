key=\"item.id\">\r\n          <td>{{ index+1 }}</td>\r\n          <td>{{ item.personnelName }}</td>\r\n          <td>{{ item.personnelSurname}}</td>\r\n          <td>{{ item.personnelPatronymic }}</td> \r\n          <td>{{ item.personnelPositionDescription }}</td>\r\n          <td  style=\"width:20px\"> \r\n          <Delete v-bind:id=\"item.id\" v-bind:viewName=\"name\"/>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n  </div>\r\n\r\n</template>\r\n \r\n<script >\r\n \r\nimport Delete from '../popups/Delete';\r\n import { mapGetters, mapActions } from \"vuex\";\r\n export default {\r\n      components:{Delete},\r\n     \r\n    data () {\r\n      return {\r\n        name:'sellers',\r\n         dialog: false,\r\n       page: 1,\r\n       \r\n      }\r\n    },\r\n      methods: {\r\n    ...mapActions([\"fetchSellers\" ]),\r\n      clickFunction() {\r\n       var tr = document.querySelectorAll('.table-contract tr'); \r\n      \r\n       for (var i = 0; i < tr.length; i++) {\r\n     \r\n       \r\n         tr[i].addEventListener(\"click\", function(){   \r\n          if(document.querySelector('.table-contract tr.active_tr')!=null){\r\n           document.querySelector('.table-contract tr.active_tr').classList.remove('active_tr');}\r\n            this.classList.add('active_tr')\r\n       }, false\r\n         )}\r\n       }\r\n \r\n        },\r\n  computed: mapGetters([\"allsellers\",\"filteredworkers\"]),\r\n  created() {\r\n    this.fetchSellers()\r\n    .then(()=>{\r\n      this.clickFunction();\r\n    })\r\n     .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'){ this.$router.push({ path: '/' });   }  \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"]}]}