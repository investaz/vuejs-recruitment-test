rmData.proclamationDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.proclamationDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n        actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n        },\r\n     methods: {\r\n     ...mapActions([\"fetchContracts\", \"recoverContract\"]),\r\n       \r\n        reset() { this.$refs.form.reset(); },\r\n            \r\n       onFileSelected(event){\r\n            this.selectedFile=event.target.files[0];\r\n            this.fname= event.target.files[0].name;  \r\n      },\r\n            filesArray(){\r\n             this.allfiles.push({\r\n             id: this.idForFile,\r\n             filename:this.selectedFile,\r\n             file_des: this.description, \r\n        \r\n         })\r\n            this.uploadedDocumentFiles.push(this.selectedFile);\r\n            this.uploadedDocumentDatas.push( {description: this.description});\r\n        \r\n          this.fname='Faylı seçin'\r\n          this.selectedFile =null;\r\n            this.description ='';\r\n          this.idForFile++;\r\n          this.dialog_new=false;\r\n            },\r\n          removeFile(index) {\r\n      this.allfiles.splice(index, 1)\r\n    },\r\n\r\n       moreM(){ document.querySelector('.more-menu').style.display='none' },\r\n         recoverContractSubmit(e) { \r\n        e.preventDefault(); \r\n         const contractRecoveryFromCancelRequest = new FormData();\r\n              contractRecoveryFromCancelRequest.append('contractRecoveryFromCancelRequest', new Blob([JSON.stringify(this.formData)], {type : 'application/json'})); \r\n              contractRecoveryFromCancelRequest.append('uploadedDocumentDatas', new Blob([JSON.stringify(this.uploadedDocumentDatas)], {type : 'a