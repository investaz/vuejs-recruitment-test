lan( contractId);\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\"; \r\n import ErrorDialog from '../popups/ErrorDialog';\r\n  export default {\r\n    components:{ErrorDialog},\r\n  props:[\"contractId\", \"paymentPlanId\"],\r\n    data () {\r\n      return {\r\n         formData:{ deadline:null },\r\n         isFormValid:false,\r\n         dialog: false,\r\n         menu:false,\r\n         validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n           errorMsg:'',\r\n        dialog_err:false,\r\n      }\r\n    },\r\n    methods:{\r\n...mapActions([\"fetchPaymentPlan\", \"updatePaymentPlan\", \"getContract\", \"fetchFeeTariffs\"]),\r\n  getP(){ \r\n    this.fetchPaymentPlan(this.contractId)\r\n     .then(()=>{\r\n            this.formData=this.allpayplan.find(item => item.id === this.paymentPlanId); \r\n  \r\n          })\r\n  \r\n  },\r\n\r\n         updatePaymentSubmit(e) {   \r\n          this.formData.penaltyFeeTariffId=this.formData.penaltyFeeTariff.id;\r\n          e.preventDefault();\r\n            this.formData.contractId=this.contractId;\r\n            this.formData.paymentPlanId=this.paymentPlanId;\r\n            this.updatePaymentPlan(this.formData)\r\n              .then(()=>{ \r\n                this.fetchPaymentPlan(this.contractId);\r\n                this.getContract(this.contractId);\r\n                this.fetchFeeTariffs();\r\n                 this.dialog=false; \r\n              })  \r\n             .catch(error => {  \r\n                \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        },\r\n  },\r\n     computed: {\r\n       ...mapGetters([\"allpayplan\", \"allfeeTarifs\"]),  \r\n    \r\n     }, \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n.new-payment{\r\n  text-align: center!important;\r\n}\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.edit-but{\r\n     color: #2294f2!important;\r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n     text-transform: uppercase!important;\r\n}\r\n\r\n.btn-new-p{\r\n      padding: 0px 15px!important;\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"]}]}