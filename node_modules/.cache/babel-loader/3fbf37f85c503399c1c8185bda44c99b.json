his.loginuser,\r\n  }\r\n},\r\n     \r\n   computed:\r\n    {\r\n       ...mapGetters([\"loginuser\"]),\r\n     search:\r\n      {\r\n        get () { return this.$store.getters.search },\r\n        set (value) { this.$store.commit('updateSearch', value) }\r\n      },\r\n\r\n      \r\n      \r\n     userinfo:function(){\r\n       if(this.loginuser.user.personnelName || this.loginuser.user.personnelSurname||this.loginuser.user.personnelPatronymic){\r\n        return this.loginuser.user.personnelName+' '+this.loginuser.user.personnelSurname+' '+this.loginuser.user.personnelPatronymic;\r\n      \r\n     }\r\n     \r\n     else{\r\n       return this.loginuser.user.loginId \r\n     }\r\n     },\r\n     \r\n          \r\n   \r\n       \r\n   \r\n    },\r\n   \r\n  \r\n    created(){\r\n      console.log(this.loginuser);\r\n       \r\n    }\r\n}\r\n</script>\r\n\r\n<style >\r\n.button__badge{\r\n     background-color: #fa3e3e;\r\n    border-radius: 2px;\r\n    color: white;\r\n    padding: 1px 3px;\r\n    font-size: 10px;\r\n    position: absolute;\r\n    top: 0px;\r\n    right: 0px;\r\n    border-radius: 50%;\r\n}\r\n.theme--dark.v-sheet{\r\n  background-color: #2294F2!important;\r\n}\r\n.outline{\r\n    font-size: 11px;\r\n    padding-top: 5px;\r\n}\r\n.v-btn{\r\n    box-shadow: none;\r\n    background: transparent!important;\r\n    text-transform: unset!important;\r\n}\r\n.v-btn.v-size--default {\r\n    font-size: 10px!important;\r\n    width: auto!important;\r\n        padding: 0px 6px!important\r\n}\r\n\r\n.search{\r\n  padding: 0px 10px;\r\n}\r\n\r\n \r\n\r\n.search label{\r\n   color: #5c5c5c!important;\r\n   font-size: 16px!important;\r\n}\r\n.current_user{\r\n      font-size: 14px;\r\n    padding: 0px 10px;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}