e_but\"> \r\n                         <div style=\"text-align:center\">\r\n    <v-dialog\r\n      v-model=\"dialog_new\"\r\n      width=\"500\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        \r\n           <v-btn  icon  \r\n           text\r\n          v-on=\"on\">\r\n         \r\n           <span>SƏNƏD ƏLAVƏ ET</span> \r\n         </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span>Yeni Sənəd</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog_new = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n            <v-card-text>\r\n         \r\n           <v-row>\r\n                  <v-col  class=\"text-center inputfile\" cols=\"12\"    md=\"12\" > \r\n                       <input   multiple style=\"display:none;\" ref=\"fileInput0\" type=\"file\" @change=\"onFileSelected\"> \r\n                       <v-btn  class=\"file_button\"  @click=\"$refs.fileInput0.click()\"   label=\"File input\"> {{fname}}</v-btn>\r\n                 </v-col> \r\n                      <div class=\"clerafix\"></div>\r\n                  <v-col cols=\"12\" md=\"12\">\r\n         <v-textarea class=\"txtarea\" \r\n          solo\r\n          name=\"input-7-4\"\r\n           label=\"Təsviri\"\r\n          v-model=\"description\"\r\n          \r\n        ></v-textarea> \r\n             </v-col>\r\n            </v-row> \r\n        <v-divider></v-divider> \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn     class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn    type=\"submit\" @click=\"filesArray()\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n  \r\n        </v-card-text>\r\n      </v-card>\r\n       