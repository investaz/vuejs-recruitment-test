ge:\"\",\r\n        nameRules: [\r\n        v => !!v || 'Username is required', \r\n      ],\r\n       passwordRules: [\r\n        v => !!v || 'Password is required', \r\n      ] \r\n    }\r\n  },\r\n   computed: {\r\n    ...mapFields([\r\n      'users.loginId',\r\n      'users.password',\r\n      'users.newPassword',\r\n      'users.renewpassword'\r\n    ]),\r\n     \r\n     passwordConfirmationRule() {\r\n      return  (this.newPassword === this.renewpassword) || 'Password must match'\r\n    },\r\n \r\n  },\r\n   methods: {\r\n    login() { \r\n      this.$store.dispatch('retrieveToken', {\r\n        loginId: this.loginId,\r\n        password: this.password,\r\n      })\r\n        .then(response => {\r\n          if(response.data.user==null)\r\n          {\r\n             this.displaynewpass=true;\r\n             this.$store.dispatch('permananentPass', { \r\n             newPassword: this.newPassword, \r\n             })\r\n             .then(response => {\r\n              this.$router.push({ path: '/' })\r\n              return response  })\r\n          } \r\n          else\r\n          {\r\n              this.$router.push({ path: '/' })\r\n              return response\r\n          }\r\n          \r\n         })\r\n         .catch(error => {\r\n          //  return error;\r\n           console.log(error);\r\n           this.errorMessage='username or password is incorrect'\r\n            \r\n         })\r\n    }\r\n}\r\n}\r\n</script>\r\n\r\n<style>\r\n .login-logo{\r\n   display:inline-flex;\r\n }\r\n.login-page{\r\n  background: white;\r\n          align-items: center;\r\n          height: 100%;\r\n         width: 100%;\r\n}\r\n    .login-left, .login-right{\r\n      width:50%;\r\n      text-align: center;\r\n      display: inline-flex;\r\n    }\r\n  .login-right  \r\n  {\r\n          height: 100%; \r\n    box-shadow: 0 2px 10px 0 rgba(0, 0, 0, 0.5);\r\n    background-image: linear-gradient(to bottom, #2294f2, #164ddb);\r\n    color: white;\r\n    }\r\n\r\n    