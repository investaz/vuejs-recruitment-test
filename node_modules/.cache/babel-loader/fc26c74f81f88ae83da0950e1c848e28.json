 text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n          <v-form @submit='cancelSubmit'  v-model=\"isFormValid\" ref=\"form\" >\r\n        <v-card-text>\r\n               <v-row>\r\n                 \r\n                   <v-col cols=\"12\"  md=\"12\">\r\n                          <v-menu\r\n                              v-model=\"menu\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.cancelDate\"\r\n                              label=\"Silinmə tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker v-model=\"formatcancelDate\" @input=\"menu = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col>\r\n\r\n                   \r\n             </v-row>\r\n        </v-card-text> \r\n        <v-divider></v-divider> \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; formData.formatpaymentDate=null; formData.paymentAmount=null\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n          <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\n \r\nimport {  mapActions } from \"vuex\"; \r\n import ErrorDialog from '../popups/ErrorDialog';\r\n  export default {\r\n props:[\"id\"],\r\n components:{ErrorDialog},\r\n    data () {\r\n      return {\r\n         formData:{ },\r\n         isFormValid:false,\r\n         dialog: false,\r\n         menu:false,\r\n         errorMsg:'',\r\n         dialog_err:false,\r\n         validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n      }\r\n    },\r\n    methods:{\r\n       reset() {   this.$refs.form.reset();   },\r\n    ...mapActions([\"cancelUnitOrgs\",\"fetchMainOrgs\"]),\r\n       addc(){\r\n       var  element = document.querySelectorAll('.tf-nc~ul li'); \r\n       for (var i = 0; i < element.length; i++) {  element[i].parentElement.parentElement.firstElementChild.classList.add(\"mystyle\"); }\r\n     },\r\n      moreM(){\r\n    document.querySelector('.more-menu').style.display='none'\r\n      },\r\n      cancelSubmit(e) {   \r\n          e.preventDefault();\r\n            this.formData.organizationUnitId=this.id; \r\n            this.cancelUnitOrgs(this.formData)\r\n              .then(()=>{  \r\n                this.fetchMainOrgs()\r\n                .then(()=>{  this.addc();  }) \r\n                this.dialog=false; \r\n                \r\n              })  \r\n             .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        },\r\n     \r\n},\r\n     computed: {  \r\n            formatcancelDate:\r\n        {\r\n            get() {\r\n              if (this.formData.cancelDate === undefined) { return null; }\r\n              return  this.formData.cancelDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.cancelDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n     },\r\n  \r\n   \r\n  }\r\n</script>\r\n "],"sourceRoot":"src/popups"}]}