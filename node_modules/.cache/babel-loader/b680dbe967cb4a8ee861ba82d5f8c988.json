A,EAAA,CAAA,GAAA,EAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AAGA,QAAA,EAAA,CAAA,CAAA,CAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,YAAA;AACA,cAAA,QAAA,CAAA,aAAA,CAAA,8BAAA,KAAA,IAAA,EAAA;AACA,YAAA,QAAA,CAAA,aAAA,CAAA,8BAAA,EAAA,SAAA,CAAA,MAAA,CAAA,WAAA;AAAA;;AACA,eAAA,SAAA,CAAA,GAAA,CAAA,WAAA;AACA,SAJA,EAIA,KAJA;AAKA;AACA;AAdA,IAPA;AAuBA,EAAA,QAAA,EAAA,UAAA,CAAA,CAAA,YAAA,EAAA,iBAAA,CAAA,CAvBA;AAwBA,EAAA,OAxBA,qBAwBA;AAAA;;AACA,SAAA,YAAA,GACA,IADA,CACA,YAAA;AACA,MAAA,KAAA,CAAA,aAAA;AACA,KAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,SAAA,IAAA,kBAAA,EAAA;AAAA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AAAA;AACA,KANA;AAOA;AAhCA,CAAA","sourcesContent":["<template>\r\n<div class=\"table-componenet\">\r\n  <v-simple-table>\r\n    <template v-slot:default>\r\n      <thead>\r\n        <tr>\r\n          <th id='1' class=\"text-center\">№</th>\r\n          <th id='2'  class=\"text-center\">Adı</th>\r\n          <th id='3'  class=\"text-center\">Soyadı</th>\r\n          <th id='4'  class=\"text-center\">Doğum tarixi</th>\r\n          <th id='5'  class=\"text-center\">Vəzifəsi</th>\r\n            <th id='7'  class=\"text-center\">İşə qəbul tarixi</th> \r\n            <th id='7'  class=\"text-center\">İşdən çıxma tarixi</th>  \r\n          <th id='6'  class=\"text-center\"></th>\r\n        </tr>\r\n      </thead>\r\n      <tbody class=\"table-contract\">\r\n        <tr v-for=\"(item, index) in filteredworkers\" :key=\"item.id\">\r\n          <td>{{ index+1 }}</td>\r\n          <td>{{ item.name }}</td> \r\n          <td>{{ item.surname }}</td>\r\n          <td>{{ item.birthDate }}</td>\r\n          <td>{{ item.personnelPositionDescription }}</td>\r\n          <td>{{ item.entranceDate }}</td>\r\n          <td>{{ item.leaveDate }}</td>\r\n           <td>\r\n             <v-menu v-if='item.leaveDate==null' offset-y :close-on-content-click='false' >\r\n      <template v-slot:activator=\"{ on }\">\r\n       <v-btn v-on=\"on\" icon>\r\n        <v-icon>more_vert</v-icon>  \r\n      </v-btn>\r\n      </template>\r\n      \r\n      <v-list class='button_list'>\r\n         <v-list-item>\r\n          <NewUser v-bind:nameWorker = 'item.name'  v-bind:surnameWorker = 'item.surname' v-bind:personelId = 'item.id'  />\r\n         </v-list-item>\r\n          <v-list-item>\r\n          <LeaveWorker v-bind:personelId = 'item.id'  />\r\n         </v-list-item>\r\n      </v-list>\r\n    </v-menu></td>\r\n        </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n  </div>\r\n\r\n</template>\r\n \r\n<script >\r\nimport NewUser from '../popups/NewUser';\r\nimport LeaveWorker from '../popups/LeaveWorker';\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nexport default {\r\n    components:{NewUser,LeaveWorker},\r\n    data () {\r\n      return {\r\n          \r\n      }\r\n    },\r\n      methods: {\r\n    ...mapActions([\"fetchWorkers\", \"deleteWorker\"]),\r\n clickFunction() {\r\n       var tr = document.querySelectorAll('.table-contract tr'); \r\n      \r\n       for (var i = 0; i < tr.length; i++) {\r\n     \r\n       \r\n         tr[i].addEventListener(\"click\", function(){   \r\n          if(document.querySelector('.table-contract tr.active_tr')!=null){\r\n           document.querySelector('.table-contract tr.active_tr').classList.remove('active_tr');}\r\n            this.classList.add('active_tr')\r\n       }, false\r\n         )}\r\n       }\r\n        },\r\n  computed: mapGetters([\"allworkers\",\"filteredworkers\"]),\r\n  created() {\r\n    this.fetchWorkers()\r\n     .then(()=>{\r\n      this.clickFunction();\r\n    })\r\n     .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'){ this.$router.push({ path: '/' });   }  \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"],"sourceRoot":"src/components"}]}