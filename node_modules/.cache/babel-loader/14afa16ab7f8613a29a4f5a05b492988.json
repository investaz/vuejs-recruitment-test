eaders:[\r\n        { text: '№',   value: 'index',sortable:false },\r\n        { text: 'Adı',   value: 'personnelName'  },\r\n        { text: 'Soyadı', value: 'personnelSurname'   }, \r\n        { text: 'Ata adı',   value: 'personnelPatronymic'   },\r\n        { text: 'Vəzifəsi',   value: 'personnelPositionDescription'   },\r\n        { text: 'Sil',  value: 'delete' ,sortable:false  },\r\n         \r\n      ]\r\n       \r\n      }\r\n    },\r\n      methods: {\r\n    ...mapActions([\"fetchSellers\" ]),\r\n      rowSelect(idx) { this.selectedRow = idx; },\r\n \r\n        },\r\n  computed: mapGetters([\"allsellers\",\"filteredworkers\"]),\r\n  created() {\r\n    this.fetchSellers() \r\n     .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'){ this.$router.push({ path: '/' });   }  \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"],"sourceRoot":"src/components"}]}