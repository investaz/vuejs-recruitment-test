\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.cancelDate\"\r\n                              label=\"Ləğv tarixi\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker :first-day-of-week=\"1\" v-model=\"formatcancelDate\" @input=\"menu = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col> \r\n        \r\n \r\n             </v-row>\r\n              <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n      <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; reset()\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >LƏĞV ET</v-btn> \r\n        </v-card-actions>\r\n          </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\nimport { mapGetters, mapActions } from \"vuex\";  \r\n  export default {\r\n    components:{ErrorDialog},\r\n  props:[\"personnelPositionId\"],\r\n    data () {\r\n      return {\r\n         isFormValid:false,\r\n         formData:{},\r\n         dialog:false,\r\n         menu:false, \r\n          errorMsg:'',\r\n       dialog_err:false,\r\n      }\r\n    },\r\n\r\n     computed:{\r\n        ...mapGetters([\"allpayments\"]),\r\n           formatcancelDate:\r\n        {\r\n            get() {\r\n              if (this.formData.cancelDate === undefined) { return null; }\r\n              return  this.formData.cancelDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.cancelDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n         \r\n        \r\n        },\r\n     methods: {\r\n     ...mapActions([\"fetchPosition\", \"cancelPersonnelPos\"]),\r\n       \r\n        reset() {\r\n        this.$refs.form.reset(); \r\n      },\r\n\r\n      \r\n        cancelPersonnelPosSubmit(e) { \r\n        e.preventDefault(); \r\n        this.formData.personnelPositionId=this.personnelPositionId;\r\n        this.cancelPersonnelPos(this.formData)\r\n        .then(()=>{\r\n           this.dialog=false; \r\n           this.fetchPosition();\r\n          this.reset();\r\n        })\r\n          .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n       \r\n       }\r\n       \r\n        },\r\n     \r\n \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: uns