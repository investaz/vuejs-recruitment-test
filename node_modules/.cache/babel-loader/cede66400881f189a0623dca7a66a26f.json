           parentId: null,\r\n          readonly: true\r\n            }\r\n        ],\r\n              conttype:[{\r\n            code: \"Hamısı\",\r\n            description: \"Hamısı\",\r\n            id: [],\r\n            parentId: null,\r\n          readonly: true\r\n            }\r\n        ],\r\n          filterArray:[],\r\n          filterArrayType:[],\r\n           selectedRow: null,\r\n          name:\"contracts\",\r\n            //  formData:{},\r\n             errorMsg:'', \r\n        dialog_err:false,\r\n          menu1:false,\r\n          menu2:false,\r\n                     headers:[\r\n       { text: '№',   value: 'index',sortable:false },\r\n        { text: 'Müqavilənin tipi',   value: 'contractTypeDescription'  },\r\n        { text: 'Müqavilənin nömrəsi', value: 'contractNumber'   }, \r\n        { text: 'Satıcının A.S.A',   value: 'sellerPersonnelName'   },\r\n        { text: 'Məsul şəxsin A.S.A',  value: 'currentProcessorPersonnelName'  },\r\n        { text: 'Müqavilə tarixi',  value: 'contractDate'   },\r\n        { text: 'Statusu',   value: 'processStatus',sortable:false  },\r\n         { text: '',   value: 'more_vert',sortable:false }, \r\n      ] \r\n      }\r\n    },\r\n components:{ErrorDialog,ContractDocument,ContractPaymentPlans,Payment, CancelContract,CompleteContract, Send , HistoryContract,RecoverContract, Delete },\r\n \r\n methods: {\r\n    ...mapActions([ \"fetchContractProcessState\", \"getSearchData\",\"getContract\", \"fetchContType\" ]),\r\n\r\n  \r\n       rowSelect(idx) { this.selectedRow = idx; },\r\n         submitSearchedData(){ \r\n               for(var key in this.formData) {\r\n                 if(this.formData[key] === \"\" ) {\r\n               delete this.formData[key]\r\n        }\r\n    }\r\n           \r\n           \r\n           this.getSearchData(this.formData)\r\n            .catch(error => {   \r\n             if(error.response.data.errorCode=='Session.NotFound'|| error.response.data.errorCode=='Session.IsExpired'){\r\n                   this.$router.push({ path: '/' });   }  \r\n     \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n               })\r\n         },\r\n                     customSort: function(items, index, isDesc) {\r\n             items.sort((a, b) => {\r\n           if (index[0]=='contractDate') {\r\n            var  dda= a[index].slice(0,2);\r\n              var mma= a[index].slice(3,5);\r\n              var yya= a[index].slice(6,10);\r\n              var adate=mma+'.'+dda+'.'+yya;\r\n\r\n              var  ddb= b[index].slice(0,2);\r\n              var mmb= b[index].slice(3,5);\r\n              var yyb= b[index].slice(6,10);\r\n              var bdate=mmb+'.'+ddb+'.'+yyb;\r\n\r\n              if (!isDesc[0]) { \r\n                 return new Date(bdate).getTime() - new Date(adate).getTime();\r\n               } else { \r\n                return new Date(adate).getTime() - new Date(bdate).getTime();\r\n            }\r\n          }\r\n             else {\r\n          if (!isDesc[0]) {\r\n            return a[index] < b[index] ? -1 : 1;\r\n          } else {\r\n            return b[index] < a[index] ? -1 : 1;\r\n          }\r\n        }\r\n      });\r\n      return items;\r\n    }\r\n \r\n        },\r\n\r\n        \r\n  computed: {\r\n      ...mapGetters([ \"loginuser\",\"closeOnContentClick\", \"allcontractProcessState\",\"allsearchedContract\",\"allcontracttype\"]),\r\n   \r\n     formData:\r\n      {\r\n        get () { return this.$store.getters.comboData },\r\n        set (value) { this.$store.commit('updateCombo', value) }\r\n      },\r\n      \r\n         formatcontractDateBegin:\r\n        {\r\n            get() {\r\n              if (this.formData.contractDateBegin === undefined) { return null; }\r\n              return  this.formData.contractDateBegin.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.contractDateBegin = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n           formatcontractDateEnd:\r\n        {\r\n            get() {\r\n              if (this.formData.contractDateEnd === undefined) { return null; }\r\n              return  this.formData.contractDateEnd.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.contractDateEnd = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n       \r\n  },\r\n\r\n    created() {\r\n       this.fetchContractProcessState()\r\n       .then(()=>{\r\n         this.filterArray=this.contt.concat(this.allcontractProcessState); \r\n       });\r\n        this.fetchContType()\r\n       .then(()=>{\r\n         this.filterArrayType=this.conttype.concat(this.allcontracttype); \r\n       }) \r\n\r\n       \r\n      \r\n       } \r\n  \r\n \r\n}\r\n</script>\r\n\r\n<style >\r\n.contract_type .v-menu__content{\r\n  right: 0px!important;\r\n}\r\n.left-input{\r\n    float: left;\r\n    width: 31%;\r\n    text-align: center;\r\n}\r\n.left-input:first-child{ \r\n    width: 38%;\r\n}\r\n.buyerinfo{\r\n        border: 1px solid #c1bcbc;\r\n    display: block;\r\n    height: 32px;\r\n    padding: 3px;\r\n    border-radius: 4px;\r\n}\r\n.searchdiv .v-text-field__details{\r\n    display: none!important\r\n}\r\n\r\n.debitorbox .v-input{\r\n    margin-top: 0px!important;\r\n}\r\n\r\n.debitorbox .v-messages{\r\n    display: none!important;\r\n}\r\n\r\n/* .searchdiv .col-12{\r\n    width:20%!important;\r\n    max-width:20%!important;\r\n} */\r\n</style>\r\n "],"sourceRoot":"src/views"}]}