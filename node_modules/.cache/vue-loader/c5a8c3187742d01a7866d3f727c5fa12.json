ryDate === undefined) { return null; }\r\n              return  this.formData.recoveryDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.recoveryDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n         formatproclamationDate:\r\n        {\r\n            get() {\r\n              if (this.formData.proclamationDate === undefined) { return null; }\r\n              return  this.formData.proclamationDate.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.proclamationDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n        actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n        },\r\n     methods: {\r\n     ...mapActions([\"fetchContracts\", \"recoverContract\"]),\r\n       \r\n        reset() { this.$refs.form.reset(); },\r\n            \r\n       onFileSelected(event){\r\n            this.selectedFile=event.target.files[0];\r\n            this.fname= event.target.files[0].name;  \r\n      },\r\n            filesArray(){\r\n             this.allfiles.push({\r\n             id: this.idForFile,\r\n             filename:this.selectedFile,\r\n             file_des: this.description, \r\n        \r\n         })\r\n            this.uploadedDocumentFiles.push(this.selectedFile);\r\n            this.uploadedDocumentDatas.push( {description: this.description});\r\n        \r\n          this.fname='Faylı seçin'\r\n          this.selectedFile =null;\r\n            this.description ='';\r\n          this.idForFile++;\r\n          this.dialog_new=false;\r\n            },\r\n          removeFile(index) {\r\n      this.allfiles.splice(index, 1)\r\n    },\r\n\r\n       moreM(){ document.querySelector('.more-menu').style.display='none' },\r\n         recoverContractSubmit(e) { \r\n        e.preventDefault(); \r\n         const contractRecoveryFromCancelRequest = new FormData();\r\n              contractRecoveryFromCancelRequest.append('contractRecoveryFromCancelRequest', new Blob([JSON.stringify(this.formData)], {type : 'application/json'})); \r\n              contractRecoveryFromCancelRequest.append('uploadedDocumentDatas', new Blob([JSON.stringify(this.uploadedDocumentDatas)], {type : 'application/json'}));\r\n              contractRecoveryFromCancelRequest.contractId=this.contractId;\r\n              for(var i=0;i<this.uploadedDocumentFiles.length;i++) {\r\n               contractRecoveryFromCancelRequest.append('uploadedDocumentFiles',  this.uploadedDocumentFiles[i],  this.uploadedDocumentFiles[i].name) \r\n                 }\r\n        this.recoverContract(contractRecoveryFromCancelRequest)\r\n        .then(()=>{\r\n           this.dialog=false; \r\n          this.fetchContracts(this.actionQueryString); \r\n           this.reset()\r\n        })  \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n         })\r\n       \r\n       }\r\n       \r\n        },\r\n     \r\n  \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-butt