#2294f2\"><v-icon v-on=\"on\">perm_contact_calendar</v-icon> </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span>{{name}} üçün mövcud səlahiyyətlər</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n            icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false;chbxSelect=null;search=''\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n          <v-form  @submit=\"updateGrantSubmit\"   ref=\"form\" >\r\n                <div class='fixeddialogheader'>\r\n                     <v-row>\r\n              \r\n                    <v-col cols=\"12\"  md=\"6\"   class=\"srch\">\r\n                   <v-text-field  style=\"float: left; width: 290px;\"  v-model='search_f' solo placeholder=\"Axtar...\" ></v-text-field> \r\n                   <v-btn @click=\"search=search_f;runfetchRoleById()\" style=\"border-top-left-radius: unset;   border-bottom-left-radius: unset;   height: 34px; margin-left: -2px;\"  class=\"btn-add\" >AXTAR</v-btn>\r\n                    </v-col>\r\n                       <v-col cols=\"12\"  md=\"6\"  class=\"role-checkbox\" > \r\n                           <v-radio-group    v-model=\"chbxSelect\" :change='chbx()'>\r\n                           <v-radio value=\"1\"   label=\"Hamısını seç\" ></v-radio>  \r\n                           <v-radio value=\"0\"   label=\"Hamısını sil\"></v-radio> \r\n                        </v-radio-group>  \r\n                    </v-col>\r\n                     </v-row>\r\n                     \r\n                  <div > \r\n             </div>\r\n         </div>\r\n          <v-card-text  style=\"max-height: 45vh;  overflow: auto;\">\r\n             \r\n              <div class=\"table-componenet\">\r\n  <v-simple-table>\r\n    <template v-slot:default>\r\n          \r\n      <tbody>\r\n \r\n\r\n       \r\n        <tr v-for=\" (item,index )  in filteredViewOperations.filter(x => x.type =='MENU_ITEM')\" :key=\"index\">  \r\n          <td style=\"text-align:left!important;\">{{item.breadcrumbLabel}}</td> \r\n    \r\n          <td style=\"width:20px!important\"> <v-checkbox  name=\"chbx\"  v-model=\"menu_items\" :value=\"item.id\"></v-checkbox></td>\r\n        </tr>\r\n \r\n      <tr  v-for=\"(item,index) in filteredViewOperations.filter(x => x.type =='VIEW_COMMAND')\"  :key=\"index +'111'\"> \r\n           \r\n          <td style=\"text-align:left!important;\">{{item.breadcrumbLabel }}</td> \r\n     \r\n            <td style=\"width:20px!important\"> <v-checkbox  name=\"chbx\"  v-model=\"view_commands\" :value=\"item.id\"></v-checkbox></td>\r\n        </tr>\r\n        \r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n  </div>    \r\n        </v-card-text>   \r\n        <v-divider></v-divider>\r\n  \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; runfetchRoleById(); chbxSelect=null;search=''\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn   type=\"submit\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n     \r\n      </v-card>\r\n    </v-dialog>\r\n       <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\"; \r\n  import ErrorDialog from '../popups/ErrorDialog';\r\n  export default {\r\n      components:{ ErrorDialog},\r\n  props:[\"roleId\", \"name\"],\r\n    data () {\r\n      return {\r\n        search_f:null,\r\n        chbxSelect:null,\r\n         formData:{  },\r\n         obj:{},\r\n         menu_items:[],\r\n         view_commands:[], \r\n       errorMsg:'', \r\n        dialog_err: false,\r\n         dialog: false,\r\n         menu:false,\r\n         validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n        \r\n      }\r\n    },\r\n    methods:{\r\n...mapActions([\"fetchRoles\", \"fetchRoleById\", \"getViewOperations\", \"updateGrantRole\" ]),\r\n   runfetchRoleById(){\r\nthis.fetchRoleById(this.roleId)\r\n     .then((res)=>{\r\n       this.formData=res.data;\r\n       this.menu_items=res.data.permittedMenuItemIdList;\r\n       this.view_commands=res.data.permittedViewCommandIdList;\r\n     })\r\n   },\r\n\r\n     chbx(){\r\n       if(this.chbxSelect==0){\r\n          this.menu_items=[];\r\n           this.view_commands=[]\r\n       }\r\n       else  if(this.chbxSelect==1){\r\n         this.menu_items=this.allViewOperations.filter(x => x.type =='MENU_ITEM').map(({ id }) => id);\r\n         this.view_commands=this.allViewOperations.filter(x => x.type =='VIEW_COMMAND').map(({ id }) => id);\r\n       }\r\n     },\r\n    updateGrantSubmit(e) {   \r\n          e.preventDefault();\r\n            this.obj.roleId=this.roleId; \r\n            this.obj.menuItemIdList=this.menu_items;\r\n            this.obj.viewCommandIdList=this.view_commands;\r\n            this.updateGrantRole(this.obj)\r\n              .then(()=>{\r\n                this.dialog=false; \r\n                this.fetchRoles();  \r\n                this.runfetchRoleById(this.roleId);\r\n           })  \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n         }) \r\n        },\r\n    \r\n  },\r\n     computed: {\r\n       ...mapGetters([\"allRoles\",\"filteredViewOperations\",\"allViewOperations\"]), \r\n       \r\n     search:\r\n      {\r\n        get () { return this.$store.getters.search },\r\n        set (value) { this.$store.commit('updateSearch', value) }\r\n      }\r\n \r\n     },\r\n  \r\n   watch: { \r\n    search:  function(){\r\n      this.runfetchRoleById()\r\n    }}\r\n  }\r\n</script>\r\n\r\n<style  >\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n.new-payment{\r\n  text-align: center!important;\r\n}\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.edit-but{\r\n     color: #2294f2!important;\r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n     text-transform: uppercase!important;\r\n}\r\n\r\n.btn-new-p{\r\n      padding: 0px 15px!important;\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n.fixeddialogheader{\r\n         padding: 12px 62px 0px 30px;\r\n    -webkit-box-shadow: 0 0 13px 0 rgba(0, 0, 0, 0.15);\r\n    box-shadow: 0 0 13px 0 rgba(0, 0, 0, 0.15);\r\n    z-index: 999;\r\n    position: relative;\r\n}\r\n.role-checkbox .v-input{\r\n  margin-top: 0px!important;\r\n    float: right!important;\r\n}\r\n\r\n.fixeddialogheader .srch .v-input__slot{\r\n      border: 1px solid #e5dfdf;\r\n      box-shadow: none!important;\r\n}\r\n\r\n</style>"]}]}