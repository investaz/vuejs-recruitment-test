 file_des: this.description, \r\n        \r\n         })\r\n            this.uploadedDocumentFiles.push(this.selectedFile);\r\n            this.uploadedDocumentDatas.push( {description: this.description});\r\n        \r\n          this.fname='Faylı seçin'\r\n          this.selectedFile =null;\r\n            this.description ='';\r\n          this.idForFile++;\r\n          this.dialog_new=false;\r\n            },\r\n          removeFile(index) {\r\n      this.allfiles.splice(index, 1)\r\n    },\r\ncontractSendSubmit(e){\r\n    e.preventDefault(); \r\n      \r\n             const contractSendRequest = new FormData();\r\n              contractSendRequest.append('contractSendRequest', new Blob([JSON.stringify(this.formData)], {type : 'application/json'})); \r\n              contractSendRequest.append('uploadedDocumentDatas', new Blob([JSON.stringify(this.uploadedDocumentDatas)], {type : 'application/json'}));\r\n               contractSendRequest.contractId=this.contractId;\r\n              for(var i=0;i<this.uploadedDocumentFiles.length;i++) {\r\n               contractSendRequest.append('uploadedDocumentFiles',  this.uploadedDocumentFiles[i],  this.uploadedDocumentFiles[i].name) \r\n                 }\r\n        this.sendContract(contractSendRequest)\r\n        .then(()=>{\r\n           this.dialog=false; \r\n            this.fetchContracts(this.actionQueryString(this.router));\r\n           this.reset(); \r\n             })  \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n                this.fetchContracts(this.actionQueryString);\r\n         })\r\n}\r\n},\r\n     computed: {\r\n       ...mapGetters([\"alltasktype\",\"allusers\",\"loginuser\"]), \r\n       formatdeadline:\r\n        {\r\n            get() {\r\n              if (this.formData.deadline === undefined) { return null; }\r\n              return  this.formData.deadline.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.deadline = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n         actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n     },\r\n  created() {\r\n    this.fetchContractTaskType();\r\n     this.fetchUsers();\r\n    //  this.getContractReceivers(this.contractId);\r\n  },\r\n   \r\n     \r\n   \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"]}]}