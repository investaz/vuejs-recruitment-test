ules: [\r\n        v => !!v || 'Sahəni boş buraxmaq olmaz!', \r\n         ],\r\n      }\r\n    },\r\n\r\n     computed:{\r\n        \r\n    ...mapGetters([\"allRoles\" ]),\r\n     \r\n      },\r\n \r\n  methods: {\r\n     ...mapActions([\"fetchRoles\", \"addRole\"  ]),\r\n       reset() { this.$refs.form.reset();  }, \r\n\r\n        addRoleSubmit(e) {   \r\n        e.preventDefault(); \r\n        \r\n           this.formData.loginAllowed=true;\r\n        this.addRole(this.formData)\r\n        .then(()=>{\r\n         this.dialog=false; \r\n         this.fetchRoles();\r\n         this.reset();\r\n        })\r\n         .catch(error => {  \r\n             this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n         })\r\n        \r\n       }\r\n        },\r\n     \r\n  created() {\r\n    this.fetchRoles(); \r\n  }\r\n  }\r\n</script>\r\n\r\n "],"sourceRoot":"src/popups"}]}