         <v-form   @submit=\"contractSendSubmit\" v-model=\"isFormValid\" ref=\"form\" >\r\n        <v-card-text>\r\n               <v-row>\r\n                 \r\n                          <v-col cols=\"12\"  md=\"12\">\r\n                          <v-menu\r\n                              v-model=\"menu\"\r\n                              :close-on-content-click=\"false\"\r\n                              :nudge-right=\"40\"\r\n                               transition=\"scale-transition\"\r\n                               offset-y\r\n                               min-width=\"290px\"\r\n                          >\r\n                          <template v-slot:activator=\"{ on }\">\r\n                             <v-text-field\r\n                              :value=\"formData.deadline\"\r\n                              label=\"Son tarix\"\r\n                              append-icon=\"event\"\r\n                               readonly\r\n                               v-on=\"on\"\r\n                               class=\"required\"\r\n                               :rules=\"validationRules\"\r\n                              ></v-text-field>\r\n                          </template>\r\n                          <v-date-picker :first-day-of-week=\"1\" v-model=\"formatdeadline\" @input=\"menu = false\"></v-date-picker>\r\n                          </v-menu>\r\n                        \r\n                       </v-col>\r\n                  <v-col   cols=\"12\"   md=\"12\" > \r\n               \r\n                     <v-select  :rules=\"validationRules\"  class=\"required\"  :items=\"allrecivers\" item-value=\"id\" :item-text=\"item => item.personnelName +' '+item.personnelSurname+' '+ item.personnelPatronymic\"    v-model=\"formData.targetUserId\"   label=\"İstifadəçinin A.S.A\"></v-select>\r\n                       \r\n                 </v-col> \r\n\r\n                 <v-col   cols=\"12\"   md=\"12\" > \r\n                           <v-select  :rules=\"validationRules\"  class=\"required\" v-model=\"formData.taskTypeId\" :items=\"alltasktype\" item-value=\"id\" item-text=\"description\" label=\"Prosesi seç\" ></v-select>\r\n                      \r\n                     </v-col>\r\n\r\n                            <v-col   cols=\"12\"   md=\"12\" > \r\n               \r\n                     \r\n                   <div class=\"table-componenet\" v-if=\"allfiles.length != 0\">\r\n  <v-simple-table>\r\n    <template v-slot:default>\r\n      <thead>\r\n        <tr>\r\n          <th id='1' class=\"text-center\">№</th>\r\n          <th id='2'  class=\"text-center\">Faylın adı</th>\r\n          <th id='3'  class=\"text-center\">Təsviri</th> \r\n              \r\n          <th id='6'  class=\"text-center\">Sil</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n         \r\n        <tr v-for=\"(item, index) in allfiles\" :key=\"item.id\">\r\n          <td>{{ index+1 }}</td>\r\n          <td>{{ item.filename.name }}</td>\r\n          <td>{{ item.file_des}}</td> \r\n          <td  style=\"width:20px\"> \r\n            <v-btn @click=\"removeFile(index)\" class=\"removeFile\"><v-icon>clear</v-icon></v-btn>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n\r\n  </div> \r\n \r\n                 </v-col> \r\n                 \r\n                          <v-col   cols=\"12\"   md=\"12\" class=\"new_role_but\"> \r\n                         <div style=\"text-align:center\">\r\n    <v-dialog\r\n      v-model=\"dialog_new\"\r\n      width=\"500\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        \r\n           <v-btn  icon  \r\n           text\r\n          v-on=\"on\">\r\n         \r\n           <span>SƏNƏD ƏLAVƏ ET</span> \r\n         </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span>Yeni Sənəd</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog_new = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n            <v-card-text>\r\n         \r\n           <v-row>\r\n                  <v-col  class=\"text-center inputfile\" cols=\"12\"    md=\"12\" > \r\n                       <input   multiple style=\"display:none;\" ref=\"fileInput0\" type=\"file\" @change=\"onFileSelected\"> \r\n                       <v-btn  class=\"file_button\"  @click=\"$refs.fileInput0.click()\"   label=\"File input\"> {{fname}}</v-btn>\r\n                 </v-col> \r\n                      <div class=\"clerafix\"></div>\r\n                  <v-col cols=\"12\" md=\"12\">\r\n         <v-textarea class=\"txtarea\" \r\n          solo\r\n          name=\"input-7-4\"\r\n           label=\"Təsviri\"\r\n          v-model=\"description\"\r\n          \r\n        ></v-textarea> \r\n             </v-col>\r\n            </v-row> \r\n        <v-divider></v-divider> \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn     class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn    type=\"submit\" @click=\"filesArray()\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n  \r\n        </v-card-text>\r\n      </v-card>\r\n         \r\n    </v-dialog>\r\n  </div>\r\n                      \r\n                     </v-col>\r\n             </v-row>\r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; formData.personnelId=null\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n     <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\"; \r\n import ErrorDialog from '../popups/ErrorDialog';\r\n  export default {\r\n     props:[\"contractId\"], \r\n          components:{ ErrorDialog},\r\n\r\n    data () {\r\n      return {\r\n        router:this.$router.currentRoute.path,\r\n         description:'',\r\n        allfiles:[],\r\n        idForFile:1, \r\n        count:0,\r\n        uploadedDocumentDatas:[],\r\n        uploadedDocumentFiles:[],\r\n        errorMsg:'', \r\n        dialog_err: false,\r\n        dialog_new:false, \r\n        selectedFile:null,\r\n        fname:'Faylı seçin',\r\n        formData:{ targetUserId:null },\r\n        isFormValid:false,\r\n        dialog: false,\r\n        menu:false,\r\n        validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n      }\r\n    },\r\n    methods:{\r\n...mapActions([\"fetchContractTaskType\", \"fetchUsers\", \"sendContract\",\"fetchContracts\",\"getContractReceivers\", \"unViewedTasksCount\"]),\r\n  reset() { this.$refs.form.reset(); },\r\n\r\n       onFileSelected(event){\r\n            this.selectedFile=event.target.files[0];\r\n            this.fname= event.target.files[0].name;  \r\n      },\r\n            filesArray(){\r\n             this.allfiles.push({\r\n             id: this.idForFile,\r\n             filename:this.selectedFile,\r\n             file_des: this.description, \r\n        \r\n         })\r\n            this.uploadedDocumentFiles.push(this.selectedFile);\r\n            this.uploadedDocumentDatas.push( {description: this.description});\r\n        \r\n          this.fname='Faylı seçin'\r\n          this.selectedFile =null;\r\n            this.description ='';\r\n          this.idForFile++;\r\n          this.dialog_new=false;\r\n            },\r\n          removeFile(index) {\r\n      this.allfiles.splice(index, 1)\r\n    },\r\ncontractSendSubmit(e){\r\n    e.preventDefault(); \r\n      \r\n             const contractSendRequest = new FormData();\r\n              contractSendRequest.append('contractSendRequest', new Blob([JSON.stringify(this.formData)], {type : 'application/json'})); \r\n              contractSendRequest.append('uploadedDocumentDatas', new Blob([JSON.stringify(this.uploadedDocumentDatas)], {type : 'application/json'}));\r\n               contractSendRequest.contractId=this.contractId;\r\n              for(var i=0;i<this.uploadedDocumentFiles.length;i++) {\r\n               contractSendRequest.append('uploadedDocumentFiles',  this.uploadedDocumentFiles[i],  this.uploadedDocumentFiles[i].name) \r\n                 }\r\n        this.sendContract(contractSendRequest)\r\n        .then(()=>{ \r\n             this.fetchContracts(this.actionQueryString);\r\n             this.reset(); \r\n             \r\n             this.unViewedTasksCount(this.loginuser.user.id);\r\n             this.dialog=false; \r\n             })  \r\n        .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n                this.fetchContracts(this.actionQueryString);\r\n             \r\n         })\r\n},\r\nmoreM(){\r\n  document.querySelector('.more-menu').style.display='none'\r\n}\r\n},\r\n     computed: {\r\n       ...mapGetters([\"alltasktype\",\"allusers\",\"loginuser\",\"allrecivers\"]), \r\n       formatdeadline:\r\n        {\r\n            get() {\r\n              if (this.formData.deadline === undefined) { return null; }\r\n              return  this.formData.deadline.split('.').join('-').replace(/(\\d{2})-(\\d{2})-(\\d{4})/, \"$3-$2-$1\"); }  ,\r\n              set(value) { this.formData.deadline = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n         actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n     },\r\n  created() {\r\n   \r\n    //  this.getContractReceivers(this.contractId);\r\n  },\r\n   \r\n     \r\n   \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"],"sourceRoot":"src/popups"}]}