r\n\r\n        \r\n      },\r\n \r\n  methods: {\r\n    \r\n  \r\n     ...mapActions([\"fetchPosition\", \"addWorker\",\"fetchWorkers\", \"fetchPositionCombo\" ]),\r\n       reset() { this.$refs.form.reset();  }, \r\n\r\n        addWorkerSubmit(e) {    \r\n        e.preventDefault(); \r\n        this.addWorker(this.formData)\r\n        .then(()=>{ \r\n         \r\n         this.reset();\r\n         this.fetchWorkers()\r\n         .then(()=>{\r\n            \r\n         this.dialog=false; \r\n         })\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n        \r\n       },\r\n\r\nfetchEmptyPos(){\r\n  this.fetchPositionCombo()\r\n  .then((res)=>{\r\n     this.emptyP=res.data.filter(pos=>{return pos.personnelId === undefined});\r\n     console.log(res.data);\r\n  })\r\n}\r\n\r\n        },\r\n     \r\n mounted(){\r\n      $(document).ready(function() {\r\n        $(\".datepicker\").datetimepicker ({\r\n            autoclose: true,\r\n            todayHighlight: true,\r\n            useCurrent: false,\r\n            language: 'az'\r\n        }).datetimepicker ();\r\n    });\r\n }\r\n  }  \r\n  \r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25