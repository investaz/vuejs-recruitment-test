tters([\"allRoles\",\"allViewOperations\"]), \r\n       \r\n\r\n    \r\n     },\r\n     \r\n   created(){\r\n     this.fetchRoles(); \r\n     this.getViewOperations();\r\n     this.runfetchRoleById()\r\n   }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n.new-payment{\r\n  text-align: center!important;\r\n}\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.edit-but{\r\n     color: #2294f2!important;\r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n     text-transform: uppercase!important;\r\n}\r\n\r\n.btn-new-p{\r\n      padding: 0px 15px!important;\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"]}]}