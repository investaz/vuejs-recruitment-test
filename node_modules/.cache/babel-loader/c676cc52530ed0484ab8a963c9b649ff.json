elect(idx) { this.selectedRow = idx; },\r\n        },\r\n  computed: mapGetters([\"allPosition\"]),\r\n  created() {\r\n    this.fetchPosition() \r\n        .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'|| error.response.data.errorCode=='Session.IsExpired'){ this.$router.push({ path: '/' });   }\r\n         \r\n              this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n        \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"],"sourceRoot":"src/components"}]}