            </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n         <v-card-title class=\"modal-header\">\r\n          <span>Sənədlər</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n            icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n         \r\n        <v-card-text>\r\n         \r\n                \r\n                                      \r\n        \r\n                            <v-row>\r\n\r\n                               <v-col   cols=\"12\"   md=\"12\"  v-if=\"allDocuments.length === 0\" class=\"norecord\">\r\n          <div style=\"text-align:center\"> Sənəd əlavə olunmayıb...</div>\r\n        </v-col>\r\n                  <v-col  v-if=\"allDocuments.length != 0\"   cols=\"12\"   md=\"12\" > \r\n               \r\n                     \r\n                   <div class=\"table-componenet\">\r\n  <v-simple-table>\r\n    <template v-slot:default>\r\n      <thead>\r\n        <tr>\r\n          <th id='1' class=\"text-center\">№</th>\r\n          <th id='1' class=\"text-center\">Məsul şəxsin A.S.A</th> \r\n          <th id='2'  class=\"text-center\">Faylın adı</th>\r\n          <th id='3'  class=\"text-center\">Təsviri</th> \r\n              \r\n          <th  v-if=\"router!='/deletedcontracts'&&router!= '/archive'\" id='6'  class=\"text-center\">Sil</th>\r\n            <th id='6'  class=\"text-center\">Yüklə</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n       \r\n        \r\n        <tr v-for=\"(item, index) in allDocuments\" :key=\"item.id\">\r\n          <td>{{ index+1 }}</td>\r\n          <td>{{ item.uploadedByPersonnelName + ' '+item.uploadedByPersonnelSurname+' '+item.uploadedByPersonnelPatronymic }}</td>\r\n          <td>{{ item.originalFileName}}</td> \r\n          <td>{{ item.description}}</td> \r\n          <td  v-if=\"router!='/deletedcontracts'&&router!= '/archive'\" style=\"width:20px\"> \r\n              <Delete v-bind:id=\"item.id\" v-bind:viewName=\"name\"  v-bind:contractId=\"contractId\" />  \r\n          </td>\r\n           <!-- <td v-if=\"item.originalFileName && (item.originalFileName.split('.').pop()==='pdf'||item.originalFileName.split('.').pop()==='txt'||item.contentType.split('/').shift()==='image'||item.contentType.split('/').shift()==='xls')\"  style=\"width:20px\"> \r\n             <IframeDoc v-bind:id=\"item.id\" v-bind:contractId=\"contractId\"/>  \r\n          \r\n          </td> -->\r\n          <td  class=\"down_file\"><a target=\"_blank\" :href='`http://192.168.65.249:8080/Contracts/${contractId}/Documents/${item.id}/File`'><v-icon>vertical_align_bottom</v-icon></a></td>\r\n    \r\n        </tr>  \r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n \r\n  </div> \r\n \r\n                 </v-col> \r\n    \r\n       \r\n             </v-row> \r\n            <v-col  cols=\"12\"   md=\"12\" class=\"new_role_but\">\r\n    <v-dialog\r\n      v-model=\"dialog_new\"\r\n      width=\"500\"\r\n      v-if=\"router!='/deletedcontracts'&&router!= '/archive'\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        \r\n          <v-btn  icon  \r\n           text\r\n          v-on=\"on\">\r\n         \r\n           <span>SƏNƏD ƏLAVƏ ET</span> \r\n         </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span>Yeni Sənəd</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog_new = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n            <v-card-text>\r\n         \r\n           <v-row>\r\n            \r\n                  <v-col  class=\"text-center inputfile\" cols=\"12\"    md=\"12\" > \r\n                       <input   multiple style=\"display:none;\" ref=\"fileInput0\" type=\"file\" @change=\"onFileSelected\"> \r\n                       <v-btn  class=\"file_button\"  @click=\"$refs.fileInput0.click()\"   label=\"File input\"> {{fname}}</v-btn>\r\n                 </v-col> \r\n                      <div class=\"clerafix\"></div>\r\n                  <v-col cols=\"12\" md=\"12\">\r\n         <v-textarea class=\"txtarea\" \r\n          solo\r\n          name=\"input-7-4\"\r\n           label=\"Təsviri\"\r\n          v-model=\"description\" \r\n          \r\n        ></v-textarea> \r\n             </v-col>\r\n            </v-row> \r\n        <v-divider></v-divider> \r\n     \r\n   <v-card-actions class='dialog-buttons'>\r\n           <v-btn     @click=\"selectedFile=null; description=''; fname='Faylı seçin';dialog_new = false;\"   class=\"btn-cancel\" outlined>IMTINA ET</v-btn> \r\n             <v-btn  @click=\"onUpload()\" :disabled=\"fname==null ||fname=='Faylı seçin'\"  class=\"btn-add\" >ƏLAVƏ ET</v-btn> \r\n             \r\n        </v-card-actions>\r\n        </v-card-text>\r\n             <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n      </v-card>\r\n        \r\n    </v-dialog>\r\n  </v-col>\r\n             \r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n       \r\n     <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false;\"  class=\"btn-cancel\" outlined>BAĞLA</v-btn> \r\n            \r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n</template>\r\n\r\n<script>\r\n// import IframeDoc from '../popups/IframeDoc';\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\n import Delete from '../popups/Delete';\r\n import { mapGetters, mapActions } from \"vuex\";\r\n export default {\r\n      components:{Delete,ErrorDialog },\r\n    props:[\"contractId\", \"router\"],\r\n   data(){\r\n       return{\r\n         downl_file:'',\r\n         name:\"documents\",\r\n            count:0,\r\n            dialog_new:false,\r\n            dialog: false,\r\n            dialog_fayl: false,\r\n            selectedFile:null,\r\n            fname:'Faylı seçin', \r\n            description:'',\r\n            errorMsg:'',\r\n        dialog_err:false,  \r\n        obj:{},      \r\n       }\r\n   },\r\n   computed: { ...mapGetters([\"allDocuments\", \"loginuser\"]),\r\n  \r\n   },\r\n   methods:{\r\n\r\n        ...mapActions([\"fetchContractDocuments\", \"addContractDocuments\", \"downloadContractDocument\"]),\r\n           onFileSelected(event){\r\n            this.selectedFile=event.target.files[0];\r\n            this.fname= event.target.files[0].name; \r\n\r\n          \r\n      }, \r\nmoreM(){\r\n  document.querySelector('.more-menu').style.display='none'\r\n},\r\n      downloadFile(id){\r\n        this.obj.contractId=this.contractId;\r\n        this.obj.documentId=id;\r\n        this.downloadContractDocument(this.obj)\r\n         .then((response)=>{\r\n               this.downl_file=response.data\r\n               \r\n               }) \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n         })\r\n      },\r\n      onUpload(){\r\n           \r\n           const uploadedFileDatas = new FormData();\r\n               \r\n              uploadedFileDatas.append('uploadedDocumentData', new Blob([JSON.stringify( {description: this.description})], {type : 'application/json'}));  \r\n              uploadedFileDatas.append('uploadedDocumentFile', this.selectedFile, this.selectedFile.fname);  \r\n\r\n               uploadedFileDatas.contractId=this.contractId;\r\n              this.addContractDocuments(uploadedFileDatas)\r\n               .then(()=>{\r\n               this.dialog_new=false;\r\n               \r\n                this.fetchContractDocuments(this.contractId);\r\n                this.selectedFile=null;\r\n                this.description=\"\";\r\n                this.fname='Faylı seçin';\r\n               })  \r\n                .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                this.dialog_new=false; \r\n         })\r\n\r\n \r\n             \r\n            },\r\n            \r\n\r\n       \r\n   \r\n   },\r\n    created(){\r\n     \r\n    },\r\n    \r\n   \r\n  \r\n}\r\n</script>\r\n\r\n <style  >\r\n .down_file a{\r\n   text-decoration: none!important;\r\n }\r\n\r\n .down_file a i{\r\n      color: #14b6db!important;\r\n }\r\n.iframe_dialog img{\r\n  width:100%!important;\r\n   height:100%!important;\r\n}\r\n #list .row{\r\n   align-items: center;\r\n }\r\n\r\n  .txtarea label{\r\n    font-size: 12px!important;\r\n }\r\n .file_button{\r\n       width: 100%!important;\r\n    border: 1px solid #e4e4e3;\r\n        font-size: 16px!important;\r\n    letter-spacing: unset!important;\r\n \r\n    color: rgba(0, 0, 0, 0.6);\r\n    overflow: hidden;\r\n }\r\n.file_add{ \r\n    margin-top: 10px;\r\n    text-align: right;\r\n}\r\n.file_add button{\r\n    background: #14b6db!important;\r\n    color: white;}\r\n \r\n#itmList{\r\n   border: 1px solid #ecebeb;\r\n   margin-bottom: 8px;\r\n}\r\n\r\n.toAdd{\r\n  display: none;\r\n}\r\n\r\n.downloadFile{\r\n      min-width: auto!important;\r\n      color:#14b6db!important;\r\n}\r\n </style>"],"sourceRoot":"src/popups"}]}