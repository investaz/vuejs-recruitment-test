n        },\r\n  computed: mapGetters([\"allworkers\",\"filteredworkers\"]),\r\n  created() {\r\n    this.fetchWorkers()\r\n     .then(()=>{\r\n      this.clickFunction();\r\n    })\r\n     .catch(error => {  \r\n        if(error.response.data.errorCode=='Session.NotFound'){ this.$router.push({ path: '/' });   }  \r\n         })\r\n  }\r\n  \r\n}\r\n</script>"]}]}