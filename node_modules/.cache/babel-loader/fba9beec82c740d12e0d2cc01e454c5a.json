CAMA,UAAA,KAAA,EAAA;AAEA,QAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAXA;AAYA,KAxFA;AAyFA,IAAA,UAzFA,wBAyFA;AAAA;;AAEA,WAAA,gBAAA,CAAA,KAAA,EAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AAEA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AAEA,OAVA;AAWA;AAtGA;AAXA,CAAA","sourcesContent":["<template>\r\n      <v-dialog\r\n      v-model=\"dialog\"\r\n      width=\"350\" \r\n    >\r\n      <template v-slot:activator=\"{ on }\"  >\r\n           <v-btn v-on=\"on\"  icon :class=\"{ hrmorevert: viewName==='contracts'}\" @click=\"moreM()\">\r\n             <v-icon  color=\"red\">delete</v-icon> <span  v-if='viewName===\"contracts\"'>Sil</span> </v-btn>\r\n\r\n            \r\n      </template>\r\n\r\n      <v-card>\r\n         <v-card-title class=\"modal-header\">\r\n          <span>Sil</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n            icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n         \r\n        <v-card-text>\r\n               <v-row>\r\n                  <v-col  class=\"text-center\" cols=\"12\"   md=\"12\" > \r\n                        <div style=\"padding-top:20px\">Silmək istədiyinizə əminsinizmi?</div> \r\n                 </v-col> \r\n\r\n                  <v-col   v-if='viewName===\"contracts\"'  class=\"cont-del text-center\" cols=\"12\"   md=\"12\"  >\r\n                        <v-checkbox \r\n                              label=\"Silinənlər siyahısında görünsün\"\r\n                              v-model=\"obj.switchOnlyStatus\"\r\n                         ></v-checkbox>  \r\n                    </v-col> \r\n             </v-row>\r\n        </v-card-text>\r\n \r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn     @click=\"dialog = false\"   class=\"btn-cancel\" outlined>XEYR</v-btn> \r\n             <v-btn v-if='viewName===\"sellers\"'  @click=\"deleteSeller(id)\"   class=\"btn-add\" >BƏLİ</v-btn> \r\n            <v-btn v-if='viewName===\"dictionary\"'  @click=\"deleteDictionary()\"   class=\"btn-add\" >BƏLİ</v-btn> \r\n             <v-btn v-if='viewName===\"userrole\"'  @click=\"delUser()\"   class=\"btn-add\" >BƏLİ</v-btn>\r\n              <v-btn v-if='viewName===\"contracts\"'  @click=\"delContract()\"   class=\"btn-add\" >BƏLİ</v-btn>\r\n            <v-btn v-if='viewName===\"feeTarriffs\"'  @click=\"deleteFeet()\"   class=\"btn-add\" >BƏLİ</v-btn>\r\n              <v-btn v-if='viewName===\"documents\"'  @click=\"deleteDoc()\"   class=\"btn-add\" >BƏLİ</v-btn>\r\n            <v-btn v-if='viewName===\"payPlan\"'  @click=\"deletePlan()\"   class=\"btn-add\" >BƏLİ</v-btn> \r\n        </v-card-actions>\r\n    \r\n      </v-card>\r\n       <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n    </v-dialog>\r\n    \r\n</template>\r\n\r\n<script>\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\nimport { mapActions } from \"vuex\";  \r\n  export default {\r\n    components:{ErrorDialog},\r\n   props:['id', 'viewName', 'slctedItem',\"userId\", 'contractId'],\r\n    data () {\r\n      return {\r\n         dialog: false,\r\n         obj:{ switchOnlyStatus:true},\r\n         errorMsg:'',\r\n        dialog_err:false,\r\n      }\r\n    },\r\n      methods: {\r\n    ...mapActions([\"fetchPaymentPlan\",\"deletePayPlan\",\"deleteDocument\",\"deleteSeller\",\"deleteDictionaryItem\", \"deleteUserRoles\",\"fetchUserRoles\",\"deleteContract\", \"deleteFeeTariffs\" ]),\r\n\r\n       deleteDictionary() {  \r\n        this.obj.dictionaryCode=this.slctedItem;\r\n        this.obj.dictionaryItemId=this.id;\r\n        this.deleteDictionaryItem(this.obj)\r\n        .then( response=>{\r\n                this.dialog=false;\r\n                 return response\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n         })\r\n        \r\n        \r\n       },\r\n\r\n       delUser(){\r\n         this.obj.userId=this.userId;\r\n         this.obj.roleIds=this.id;\r\n        this.deleteUserRoles(this.obj)\r\n        .then(()=>{\r\n          this.dialog=false;\r\n           this.fetchUserRoles(this.userId);\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n         })\r\n       },\r\n\r\n     delContract(){\r\n         this.obj.contractId=this.contractId;\r\n        \r\n        this.deleteContract(this.obj)\r\n        .then(()=>{\r\n          this.dialog=false;\r\n           \r\n        })\r\n           .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n         })\r\n        \r\n       },\r\n      moreM(){\r\n        if(document.querySelector('.more-menu')){\r\n         document.querySelector('.more-menu').style.display='none'\r\n         }\r\n         },\r\n         deleteDoc(){\r\n         this.obj.contractId=this.contractId;\r\n         this.obj.documentId=this.id;\r\n     \r\n\r\n        this.deleteDocument(this.obj)\r\n        .then(()=>{\r\n          this.dialog=false;\r\n           \r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n         })\r\n       },\r\n      deletePlan(){\r\n         this.obj.contractId=this.contractId;\r\n         this.obj.paymentPlanId=this.id;\r\n     \r\n\r\n        this.deletePayPlan(this.obj)\r\n        .then(()=>{\r\n              this.$emit('onSubmit');\r\n          this.dialog=false;\r\n              this.fetchPaymentPlan(this.contractId);\r\n        })\r\n         .catch(error => {  \r\n           \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n         })\r\n       },\r\n       deleteFeet(){\r\n           \r\n        this.deleteFeeTariffs(this.id)\r\n        .then(()=>{\r\n          this.dialog=false;\r\n           \r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data; \r\n                    this.dialog=false; \r\n                    \r\n         })\r\n       },\r\n        },\r\n   \r\n  \r\n}\r\n</script>\r\n<style>\r\n.cont-del .v-input__control{\r\n  margin:auto!important;\r\n}\r\n</style>"],"sourceRoot":"src/popups"}]}