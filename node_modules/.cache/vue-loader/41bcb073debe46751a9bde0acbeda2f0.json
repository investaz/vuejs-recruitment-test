\n          <v-icon>archive</v-icon>\r\n          <span>Bitir</span>\r\n         </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n         <v-card-title class=\"modal-header\">\r\n          <span>Bitir</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n            icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n         \r\n        <v-card-text>\r\n               <v-row>\r\n                  <!-- <v-col  class=\"text-center\" cols=\"12\"   md=\"12\" > \r\n                        <div style=\"padding-top:20px\">Müqaviləni bitirmək istədiyinizə əminsinizmi?</div> \r\n                 </v-col>  -->\r\n\r\n                  <v-col   cols=\"12\"   md=\"12\" > \r\n               \r\n                     \r\n                   <div class=\"table-componenet\" v-if=\"allfiles.length != 0\">\r\n  <v-simple-table>\r\n    <template v-slot:default>\r\n      <thead>\r\n        <tr>\r\n          <th id='1' class=\"text-center\">№</th>\r\n          <th id='2'  class=\"text-center\">Faylın adı</th>\r\n          <th id='3'  class=\"text-center\">Təsviri</th> \r\n              \r\n          <th id='6'  class=\"text-center\">Sil</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n         \r\n        <tr v-for=\"(item, index) in allfiles\" :key=\"item.id\">\r\n          <td>{{ index+1 }}</td>\r\n          <td>{{ item.filename.name }}</td>\r\n          <td>{{ item.file_des}}</td> \r\n          <td  style=\"width:20px\"> \r\n            <v-btn @click=\"removeFile(index)\" class=\"removeFile\"><v-icon>clear</v-icon></v-btn>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-simple-table> \r\n\r\n  </div> \r\n \r\n                 </v-col> \r\n                 \r\n                          <v-col   cols=\"12\"   md=\"12\" class=\"new_role_but\"> \r\n                         <div style=\"text-align:center\">\r\n    <v-dialog\r\n      v-model=\"dialog_new\"\r\n      width=\"500\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        \r\n           <v-btn  icon  \r\n           text\r\n          v-on=\"on\">\r\n         \r\n           <span>SƏNƏD ƏLAVƏ ET</span> \r\n         </v-btn>\r\n      </template>\r\n\r\n      <v-card>\r\n        <v-card-title\r\n          class=\"modal-header\"\r\n           \r\n        >\r\n          <span>Yeni Sənəd</span>\r\n          <v-spacer></v-spacer>\r\n          \r\n           <v-btn\r\n           icon\r\n            color=\"primary\"\r\n            text\r\n            @click=\"dialog_new = false\"\r\n          ><v-icon>cancel</v-icon>\r\n            \r\n          </v-btn>\r\n          \r\n        </v-card-title>\r\n            <v-card-text>\r\n         \r\n           <v-row>\r\n                  <v-col  class=\"text-center inputfile\" cols=\"12\"    md=\"12\" > \r\n                       <input   multiple style=\"display:none;\" ref=\"fileInput0\" type=\"file\" @change=\"onFileSelected\"> \r\n                       <v-btn  class=\"file_button\"  @click=\"$refs.fileInput0.click()\"   label=\"File input\"> {{fname}}</v-btn>\r\n                 </v-col> \r\n                      <div class=\"clerafix\"></div>\r\n                  <v-col cols=\"12\" md=\"12\">\r\n         <v-textarea class=\"txtarea\" \r\n          solo\r\n          name=\"input-7-4\"\r\n           label=\"Təsviri\"\r\n          v-model=\"description\"\r\n          \r\n        ></v-textarea> \r\n             </v-col>\r\n            </v-row> \r\n        <v-divider></v-divider> \r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn     class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn    type=\"submit\" @click=\"filesArray()\" class=\"btn-add\" >TƏSDİQ ET</v-btn> \r\n        </v-card-actions>\r\n  \r\n        </v-card-text>\r\n      </v-card>\r\n         \r\n    </v-dialog>\r\n  </div>\r\n                      \r\n                     </v-col>\r\n             </v-row>\r\n        </v-card-text>\r\n <ErrorDialog v-model=\"dialog_err\"  v-bind:errorMsg=\"errorMsg\"/>\r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn     @click=\"dialog = false\"   class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn    @click=\"cmpCont()\"   class=\"btn-add\" >TƏSDİQ ET</v-btn>  \r\n        </v-card-actions>\r\n    \r\n      </v-card>\r\n    </v-dialog>\r\n</template>\r\n<script>\r\n import ErrorDialog from '../popups/ErrorDialog';\r\nimport {  mapActions , mapGetters} from \"vuex\";  \r\n  export default {\r\n    props:[\"contractId\"],\r\n    components:{ ErrorDialog},\r\n    data () {\r\n      return {\r\n          router:this.$router.currentRoute.path,\r\n        description:'',\r\n        allfiles:[],\r\n        idForFile:1, \r\n        count:0,\r\n        uploadedDocumentDatas:[],\r\n        uploadedDocumentFiles:[],\r\n        errorMsg:'', \r\n        dialog_err: false,\r\n        dialog_new:false, \r\n        selectedFile:null,\r\n        fname:'Faylı seçin',\r\n\r\n     dialog:false,\r\n      }\r\n    },\r\n   computed:{\r\n        ...mapGetters([\"loginuser\"]),\r\n          actionQueryString:function(){\r\n          return this.loginuser.user.menuList.find(x => x.code ==\"TOP_MENU\").menuItemList.find(x => x.routePath ==this.router).actionQueryString;\r\n          \r\n          },\r\n        \r\n        },\r\n\r\n     methods: { ...mapActions([\"completeContract\",\"fetchContracts\"]), \r\n         \r\n       onFileSelected(event){\r\n            this.selectedFile=event.target.files[0];\r\n            this.fname= event.target.files[0].name;  \r\n      },\r\n            filesArray(){\r\n             this.allfiles.push({\r\n             id: this.idForFile,\r\n             filename:this.selectedFile,\r\n             file_des: this.description, \r\n        \r\n         })\r\n            this.uploadedDocumentFiles.push(this.selectedFile);\r\n            this.uploadedDocumentDatas.push( {description: this.description});\r\n        \r\n          this.fname='Faylı seçin'\r\n          this.selectedFile =null;\r\n            this.description ='';\r\n          this.idForFile++;\r\n          this.dialog_new=false;\r\n            },\r\n          removeFile(index) {\r\n      this.allfiles.splice(index, 1)\r\n    },\r\n    \r\n       cmpCont(){\r\n         const completeRequest = new FormData(); \r\n              completeRequest.append('uploadedDocumentDatas', new Blob([JSON.stringify(this.uploadedDocumentDatas)], {type : 'application/json'}));\r\n               completeRequest.contractId=this.contractId;\r\n              for(var i=0;i<this.uploadedDocumentFiles.length;i++) {\r\n               completeRequest.append('uploadedDocumentFiles',  this.uploadedDocumentFiles[i],  this.uploadedDocumentFiles[i].name) \r\n                 }\r\n        this.completeContract(completeRequest)\r\n        .then(()=>{\r\n          this.dialog=false; \r\n           this.fetchContracts(this.actionQueryString(this.router));\r\n        })\r\n             .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data.errorDescription; \r\n                this.dialog_new=false; \r\n         })\r\n         \r\n     }\r\n     },\r\n   \r\n    \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n.norecord{\r\n      font-style: italic;\r\n    border-bottom: 1px solid #dfdbdb; \r\n    letter-spacing: 1.88px;}\r\n</style>"]}]}