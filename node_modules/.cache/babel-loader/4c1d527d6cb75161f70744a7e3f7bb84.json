=\"Əməkdaş\"\r\n                         :items='allworkers' \r\n                        item-value=\"id\" \r\n                        \r\n                        :item-text=\"item => item.name +' '+item.surname+' '+ item.patronymic\"  \r\n                          >\r\n \r\n                    </v-select> \r\n                  \r\n \r\n            \r\n                 </v-col> \r\n             </v-row>\r\n        </v-card-text>\r\n\r\n        <v-divider></v-divider>\r\n\r\n        <v-card-actions class='dialog-buttons'>\r\n           <v-btn   @click=\"dialog = false; formData.personnelId=null\"  class=\"btn-cancel\" outlined>İMTİNA ET</v-btn> \r\n             <v-btn :disabled=\"!isFormValid\"  type=\"submit\" class=\"btn-add\" >ƏLAVƏ ET</v-btn> \r\n        </v-card-actions>\r\n    </v-form>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ErrorDialog from '../popups/ErrorDialog';\r\nimport { mapGetters, mapActions } from \"vuex\";  \r\n  export default {\r\n    components:{ErrorDialog},\r\n \r\n    data () {\r\n      return {\r\n         formData:{ personnelId:null },\r\n         isFormValid:false,\r\n             dialog: false,\r\n               errorMsg:'',\r\n        dialog_err:false,\r\n         validationRules: [ v => !!v || 'Sahəni boş buraxmaq olmaz!'],\r\n      }\r\n    },\r\n\r\n     computed:{ ...mapGetters([\"allworkers\", \"allworkers\" ])},\r\n     methods: {\r\n     ...mapActions([ \"addSeller\",\"fetchSellers\",\"fetchWorkers\" ]),\r\n         reset() {   this.$refs.form.reset();   },\r\n      \r\n        addSellerSubmit(e) {   \r\n        e.preventDefault(); \r\n        this.addSeller(this.formData)\r\n        .then(()=>{\r\n         this.dialog=false;\r\n           this.fetchSellers()\r\n          this.reset();\r\n        })\r\n         .catch(error => {  \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data;  \r\n         })\r\n         \r\n       \r\n       }\r\n        },\r\n     \r\n  created() {\r\n    this.fetchSellers(); \r\n     this.fetchWorkers(); \r\n  }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.v-btn{\r\n        text-transform: unset!important;\r\n}\r\n.modal-header{\r\n        background-color: #14b6db!important;\r\n    padding: 5px 15px!important;\r\n}\r\n.modal-header span{\r\n        font-size: 14px;\r\n    font-weight: normal;\r\n    color: white;\r\n}\r\n.modal-header .v-icon{\r\n    color: white;\r\n    font-size: 20px;\r\n}\r\n\r\n.v-dialog .v-card__actions{\r\n    justify-content: center!important;\r\n}\r\n\r\n.btn-cancel{\r\n    border: solid 1px #2294f2;\r\n    color: #2294f2;\r\n     \r\n}\r\n.btn-add{\r\n     background-color: #2294f2!important;\r\n     color: white!important;\r\n\r\n}\r\n.dialog-buttons .v-btn{\r\n  width: 115px!important;\r\n   box-shadow: unset;\r\n}\r\n.dialog-buttons{\r\n    padding: 25px 0px;\r\n}\r\n</style>"],"sourceRoot":"src/popups"}]}