  get(){ return  this.formData.buyer.proclamationDate }, \r\n              set(value) { this.formData.soldAsset.proclamationDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n        formatpowerOfAtorneyDate:\r\n        {    get(){ return  this.formData.buyer.powerOfAtorneyDate },\r\n       \r\n              set(value) { this.formData.buyer.powerOfAtorneyDate = value.replace(/(\\d{4})-(\\d{2})-(\\d{2})/, \"$3.$2.$1\"); }  \r\n       },\r\n\r\n        \r\n       ...mapGetters([\"allcontracttype\",\"getOneCont\",\"allcontractliability\",\"allsellers\",\"alltown\",\"allregion\",\"allcountry\" ]),\r\n      \r\n      \r\n   \r\n       \r\n  },\r\n\r\n    methods:{\r\n           \r\n\r\n          ...mapActions([\"fetchContracts\", \"getContract\", \"fetchSellers\",\"fetchContType\",\"fetchContractLiability\",\"fetchCity\",\"fetchCountry\",\"fetchRegion\", \"updateContract\"]),\r\n          \r\n          updateContractSubmit(e) {   \r\n          e.preventDefault();\r\n            this.formData.id=this.id;\r\n            this.updateContract(this.formData)\r\n              .then(()=>{\r\n                this.fetchContracts(\"READY_FOR_PROCESS\"); \r\n                this.$router.push({ path: '/index' });\r\n                this.getContract(this.id);\r\n              })\r\n              .catch(error =>{ \r\n                this.dialog_err=true;\r\n                this.errorMsg=error.response.data.errorDescription;\r\n                  \r\n         })\r\n\r\n            \r\n        },\r\n\r\n        getC(){\r\n          this.getContract(this.id)\r\n          .then(res=>{\r\n          \r\n               this.formData=res.data;\r\n          })\r\n        }\r\n       \r\n  },\r\n created(){\r\n\r\n   this.getContract(this.id);\r\n   this.fetchCity();\r\n   this.fetchCountry();\r\n   this.fetchRegion();\r\n   this.fetchContractLiability();\r\n   this.fetchSellers();\r\n     this.getC();\r\n      \r\n     \r\n }\r\n     \r\n  }\r\n</script>\r\n\r\n<style > \r\n.tables-tab{\r\n    box-shadow: none!important;\r\n}\r\n</style>"]}]}